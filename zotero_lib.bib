  pages = {111--125},
  publisher = {Elsevier Science},
  issn = {0145-2134},
  doi = {10.1016/0145-2134(95)00125-5},
  abstract = {Evaluated the emotional and behavioral adjustment of 41 children (aged 5.4--15.5 yrs) within the first 3 mo following the disclosure of extrafamilial sexual abuse (ESA). Ss' functioning was compared to that of a nonclinical comparison group of 43 children matched on child's age, gender, and family constellation. Child functioning was assessed using a combination of child-report, primary caregiver-report (i.e., parent), and teacher-report measures. Results revealed that sexually abused children, in comparison to nonabused children, suffered deleterious and clinically significant effects. Standard multiple regressions found that Ss' perceptions of self-blame and guilt for the abuse and the extent of traumatization predicted their self-reported symptomatology of depression, social efficacy, and general and abuse-related fears. Also, Ss' gender predicted the level of general fearfulness. None of the other demographic or abuse-related variables were related to children's functioning. It is concluded that these results underscore the need for multidimensional and multisource assessment of children who experience ESA, and point to the clinical importance of addressing the abuse-related attributions of these children. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  keywords = {5.4-15.5 yr olds,Adolescent,Affective Symptoms,Child,Child Abuse,Child Abuse Sexual,Child Preschool,Child Reactive Disorders,emotional & behavioral adjustment following disclosure of extrafamilial sexual abuse,Emotional Adjustment,Family,Female,Gender Identity,Humans,Male,Personality Assessment,Self Disclosure,Self-Disclosure,Sexual Abuse}
}

@article{limAntecedentsSexualNonsexual1998,
  title = {Antecedents of Sexual and Non-Sexual Aggression in Young {{Singaporean}} Men},
  author = {Lim, Sandy and Howard, Rick},
  year = {1998},
  month = dec,
  journal = {Personality and Individual Differences},
  volume = {25},
  number = {6},
  pages = {1163--1182},
  issn = {0191-8869},
  doi = {10.1016/S0191-8869(98)00101-9},
  urldate = {2021-05-07},
  abstract = {This study investigated factors that have previously been implicated in male sexual aggression towards women, using a structural equation modelling approach to assess their relative influence on sexually coercive behaviours in young Singaporean men. Variables were classified under three major headings: childhood experience, personality, and attitudes facilitating violence. Non-sexual aggression against women was included in order to investigate its relationship to sexual aggression as well as its own causal antecedents. Results suggest that sexual and non-sexual aggression have different antecedents, and confirm the primacy of sexual variables (sexual experience, child sexual abuse) as antecedents to sexual aggression. Contrary to the hypothesis that sexual aggression is primarily aggressive in motivation, belligerence exerted only indirect effects on sexual aggression. Attitudinal variables (e.g. attitudes supportive of sexual aggression) contributed modestly to sexual but not to non-sexual aggression. Antisociality exerted indirect effects on both sexual aggression (via sexual experience and acceptance of interpersonal violence) and on non-sexual aggression (via belligerence). Parental violence contributed exclusively to non-sexual aggression. Finally, some cross-cultural differences in sexually coercive behaviour are highlighted.},
  langid = {english},
  keywords = {Non-sexual aggression,Sexual aggression,Women},
  file = {/Users/sbward/Zotero/storage/J9N3SRW4/Lim and Howard - 1998 - Antecedents of sexual and non-sexual aggression in.pdf;/Users/sbward/Zotero/storage/R5FDGDRJ/S0191886998001019.html}
