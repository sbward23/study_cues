@phdthesis{radomskiCognitiveProcessingAlcoholInvolved2018,
  title = {Cognitive {{Processing Alcohol-Involved Sexual Assault}}: {{An Application}} of {{Meaning Making Theory}}},
  shorttitle = {Cognitive {{Processing Alcohol-Involved Sexual Assault}}},
  author = {Radomski, Sharon},
  year = {2018},
  address = {United States -- New York},
  urldate = {2021-08-22},
  abstract = {Sexual assault is most commonly experienced during young adulthood, a period that contains important developmental milestones and identity exploration. Also characteristic of this time period are high rates of alcohol use. Notably, about half of assaults during this period involve alcohol, and variability in post-assault adjustment may be influenced by alcohol involvement. Moreover, theory suggests that whether and how one comes to make sense of the event (i.e., meaning making) is critical to post-assault functioning. Victim alcohol use at the time of the assault may present particular challenges in meaning making; yet, this has not been well examined. This is a gap in the knowledge base which, if addressed, has the potential to inform interventions. Accordingly, in a sample of young adult females who have experienced a sexual assault in the past year, the present study used a convergent, mixed methods design to improve understanding of the role of victim alcohol use in post-assault adaptation meaning-making. Quantitative (i.e., self-report measures) and qualitative (i.e., assault narrative and impact statement) methods were used to address the following three aims: 1) examine differences in meaning making based on assault type group (substance vs non-substance involved), 2) evaluate the role of peritraumatic variables (e.g., freezing response, resistance, trauma memory), and 3) test meaning making as a mediator of the association between assault type and post-assault functioning more broadly. Findings generally did not support differences in meaning making or post-assault functioning based on victim alcohol use; however the significance of peritraumatic variables and meaning making was demonstrated. Victim alcohol use may influence assault recovery when considered in the context of other peritraumatic variables. Findings may have important implications for targeted inventions aimed at improving post-assault functioning in victims of sexual assault experienced during young adulthood.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  isbn = {9780438455757},
  langid = {english},
  school = {State University of New York at Buffalo},
  keywords = {Alcohol,Alcohol-involved trauma,cognitive,Health and environmental sciences,Meaning making,Psychology,Sexual assault},
  file = {/Users/sbward/Zotero/storage/K8RC8VJS/Radomski - Cognitive Processing Alcohol-Involved Sexual Assau.pdf}
}

@article{rahm-kniggeIdentifyingRelationshipsDifficulties2023,
  title = {Identifying {{Relationships Between Difficulties}} with {{Emotion Regulation}} and {{Compulsive Sexual Behavior}}},
  author = {{Rahm-Knigge}, Ryan L. and Gleason, Neil and Mark, Kristen and Coleman, Eli},
  year = {2023},
  month = sep,
  journal = {Archives of Sexual Behavior},
  issn = {1573-2800},
  doi = {10.1007/s10508-023-02690-8},
  abstract = {Emotion dysregulation is an associated feature of compulsive sexual behavior disorder (CSBD), now recognized by the World Health Organization. Previous studies have identified associations between trait emotion dysregulation and CSBD. Given that difficulties with emotion regulation (DERS) is comprised of several facets (e.g., difficulty with impulse control and lacking awareness of one's feelings when upset), and that these facets differentially relate to other mental health concerns, the present study aimed to examine how DERS facets relate to compulsive sexual behavior (CSB). The present study also considered interpersonal emotion regulation via attachment avoidance and attachment anxiety. Hierarchical regression was conducted, first accounting for demographic covariates, then adding attachment styles, and finally all DERS subscales. Results indicated that, among a large, diverse, online U.S. sample (N = 915; Mage\,=\,39.21, SD\,=\,0.81; 54.3\% men), difficulty controlling impulses when upset, difficulty with clarity of emotions, and non-acceptance of emotions were significantly positively associated with CSB (small to moderate effects). Attachment anxiety and avoidance were also significantly positively associated with CSB, although their effects were minimal when adding DERS facets. Overall, this study supports the theorized impact of emotion dysregulation on CSB. Assessment of individual differences in DERS and intervening on these concerns are important for treating CSB.},
  langid = {english},
  pmid = {37670204},
  keywords = {Compulsive sexual behavior,Difficulties with emotion regulation,DSM-5,Emotion dysregulation,ICD-11,Sexual health}
}

