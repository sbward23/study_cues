@article{gilmoreYouExpectMe2016,
  title = {``{{Do}} You Expect Me to Receive {{PTSD}} Care in a Setting Where Most of the Other Patients Remind Me of the Perpetrator?'': {{Home-based}} Telemedicine to Address Barriers to Care Unique to Military Sexual Trauma and Veterans Affairs Hospitals},
  shorttitle = {``{{Do}} You Expect Me to Receive {{PTSD}} Care in a Setting Where Most of the Other Patients Remind Me of the Perpetrator?},
  author = {Gilmore, Amanda K. and Davis, Margaret T. and Grubaugh, Anouk and Resnick, Heidi and Birks, Anna and Denier, Carol and Muzzy, Wendy and Tuerk, Peter and Acierno, Ron},
  year = {2016},
  month = may,
  journal = {Contemporary Clinical Trials},
  volume = {48},
  pages = {59--64},
  issn = {15517144},
  doi = {10.1016/j.cct.2016.03.004},
  urldate = {2021-08-09},
  abstract = {Home-based telemedicine (HBT) is a validated method of evidence-based treatment delivery for posttraumatic stress disorder (PTSD), and justification for its use has centered on closing gaps related to provider availability and distance to treatment centers. However, another potential use of HBT may be to overcome barriers to care that are inherent to the treatment environment, such as with female veterans who have experienced military sexual trauma (MST) and who must present to VA Medical Centers where the majority of patients share features with perpetrator (e.g. gender, clothing) and may function as reminders of the trauma. Delivering evidence-based therapies to female veterans with MST-related PTSD via HBT can provide needed treatment to this population. This manuscript describes an ongoing federally funded randomized controlled trial comparing Prolonged Exposure (PE) delivered in-person to PE delivered via HBT. Outcomes include session attendance, satisfaction with services, and clinical and quality of life indices. It is hypothesized that based on intent-to-treat analyses, HBT delivery of PE will be more effective than SD at improving both clinical and quality of life outcomes at post, 3-, and 6-month follow-up. This is because `dose received', that is fewer sessions missed, and lower attrition, will be observed in the HBT group. Although the current manuscript focuses on female veterans with MST-related PTSD, implications for other populations facing systemic barriers are discussed.},
  langid = {english},
  file = {/Users/sbward/Zotero/storage/E7MFC8NG/Gilmore et al. - 2016 - “Do you expect me to receive PTSD care in a settin.pdf}
}

@article{ginzburgAbuserelatedBeliefsQuestionnaire2006,
  title = {The Abuse-Related Beliefs Questionnaire for Survivors of Childhood Sexual Abuse},
  author = {Ginzburg, Karni and Arnow, Bruce and Hart, Stacey and Gardner, William and Koopman, Cheryl and Classen, Catherine C. and {Giese-Davis}, Janine and Spiegel, David},
  year = {2006},
  month = aug,
  journal = {Child Abuse \& Neglect},
  volume = {30},
  number = {8},
  pages = {929--943},
  issn = {0145-2134},
  doi = {10.1016/j.chiabu.2006.01.004},
  urldate = {2022-08-02},
  abstract = {Objective To evaluate the psychometric properties of a new measure, the Abuse-Related Beliefs Questionnaire (ARBQ), designed to assess abuse-related beliefs among adult survivors of childhood sexual abuse (CSA). Study 1 examined the structure of the scale, and Study 2 evaluated its reliability and validity. Method One hundred and seventy female CSA survivors recruited into a group psychotherapy intervention study were administered the ARBQ in Study 1. A subsample of 45 women completed the ARBQ again 12 months later. In Study 2, 70 women from a health maintenance organization who identified themselves as survivors of CSA completed the ARBQ along with the Trauma Symptom Checklist-40 (TSC-40) and the Symptom Checklist 90R (SCL-90-R). Results Three reliable ARBQ subscales emerged in Study 1: Guilt, Shame, and Resilience. Twelve-month test-retest reliability was high (r=.60--.64). The internal consistency of the subscales was further supported in Study 2, and validity was demonstrated by moderate to high correlations with the distress measures. Conclusions The results indicate that the ARBQ has good psychometric characteristics, supporting the feasibility of its use in measuring abuse-related beliefs in research on survivors of CSA. A next step for validation of the ARBQ would be to evaluate its sensitivity in measuring changes in studies of interventions for treating CSA survivors. R{\'e}sum{\'e} Objectif {\'E}valuer les caract{\'e}ristiques psychom{\'e}triques d'un nouveau questionnaire (ARBQ) ayant pour but de mesurer les croyances qu'ont des adultes agress{\'e}s sexuellement vis-{\`a}-vis ce type d'agression. Une premi{\`e}re {\'e}tude a examin{\'e} la structure de l'{\'e}chelle tandis que la deuxi{\`e}me {\'e}tude a {\'e}valu{\'e} sa validit{\'e} et sa fiabilit{\'e}. M{\'e}thode Dans la premi{\`e}re {\'e}tude, 170 femmes victimes d'agressions sexuelles en enfance et faisant partie d'une {\'e}tude sur la psychoth{\'e}rapie de groupe ont compl{\'e}t{\'e} le questionnaire ARBQ. Une partie de l'{\'e}chantillon (45 femmes) l'ont compl{\'e}t{\'e} {\`a} nouveau 12 mois plus tard. Dans la seconde {\'e}tude, 70 femmes membres d'un organisme sur le maintien d'une bonne sant{\'e} et qui disaient avoir {\'e}t{\'e} agress{\'e}es sexuellement en enfance ont aussi compl{\'e}t{\'e} ce m{\^e}me questionnaire ainsi que deux autres questionnaires sur les sympt{\^o}mes des traumatismes (Trauma Symptom Checklist-40 et Symptom checklist 90R). R{\'e}sultats Dans la premi{\`e}re {\'e}tude, trois sous-{\'e}chelles fiables apparaissent: la culpabilit{\'e}, la honte et la r{\'e}silience. La fiabilit{\'e} du questionnaire demeure {\'e}lev{\'e}e apr{\`e}s la r{\'e}p{\'e}tition du test 12 mois plus tard. La deuxi{\`e}me {\'e}tude confirme la coh{\'e}rence interne des sous-{\'e}chelles tandis que des corr{\'e}lations importantes avec les tests qui ont mesur{\'e} la d{\'e}tresse viennent appuyer la validit{\'e} des sous-{\'e}chelles. Conclusions Comme les constats indiquent que le questionnaire ARBQ a de bonnes caracteristiques psychom{\'e}triques, il pourra servir aux chercheurs voulant mesurer les croyances vis-{\`a}-vis des agressions sexuelles. Une prochaine {\'e}tape pour valider le questionnaire serait d'{\'e}valuer s'il est sensible aux changements que conna{\^i}tront des victimes d'agressions sexuelles recevant un traitement. Resumen Objetivo Evaluar las propiedades psicom{\'e}tricas de una nueva medida, El Cuestionario de Creencias Relacionadas con el Abuso (ARBQ), dise{\~n}ado para evaluar las creencias relacionadas con el abuso en adultos sobrevivientes del abuso sexual en la ni{\~n}ez (CSA). El estudio 1 examin{\'o} la estructura de la escala, y el Estudio 2 evalu{\'o} su confiabilidad y validez. M{\'e}todo En el Estudio 1 se les administr{\'o} el ARBQ a un ciento setenta mujeres sobrevivientes del abuso sexual en la ni{\~n}ez reclutadas en un grupo de psicoterapia. Una submuestra de 45 mujeres completaron el ARBQ de nuevo, 12 meses despu{\'e}s. En el Estudio 2, 70 mujeres de una organizaci{\'o}n de salud que se identificaban como sobrevivientes del abuso sexual en la ni{\~n}ez completaron el ARBQ junto con el Inventario de S{\'i}ntomas de Trauma-40 y el Inventario de S{\'i}ntomas 90R. Resultados En el Estudio 1 se presentaron tres subescalas confiables del ARBQ: Culpa, Verg{\"u}enza y Resiliencia. La confiabilidad del test-retest doce meses despu{\'e}s fue alta (r=.60--.64). La consistencia interna de las subescalas fue adem{\'a}s apoyada en el Estudio 2, y la validez fue demostrada por correlaciones moderadas a altas con las medidas de malestar. Conclusiones Los resultados indican que el ARBQ tiene buenas caracter{\'i}sticas psicom{\'e}tricas, apoyando la posibilidad de su uso para medir creencias relacionadas con el abuso en investigaciones con sobrevivientes del abuso sexual en la ni{\~n}ez. Un pr{\'o}ximo paso para la validaci{\'o}n del ARBQ ser{\'i}a evaluar su sensibilidad para medir los cambios en estudios de intervenciones para tratar los sobrevivientes del abuso sexual en la ni{\~n}ez.},
  langid = {english},
  keywords = {Child sexual abuse (CSA),Guilt,Shame,Stigma},
  file = {/Users/sbward/Zotero/storage/2TNGMAQY/S0145213406001724.html}
}

