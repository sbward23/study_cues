@article{amone-polakPTSDMentalIllness2018,
  title = {{{PTSD}}, Mental Illness, and Care among Survivors of Sexual Violence in {{Northern Uganda}}: {{Findings}} from the {{WAYS}} Study},
  author = {{Amone-P'Olak}, Kennedy and Elklit, Ask and Dokkedahl, Sarah B{\o}gelund},
  year = {2018},
  month = may,
  journal = {Psychological Trauma: Theory, Research, Practice, and Policy},
  volume = {10},
  number = {3},
  pages = {282--289},
  publisher = {Educational Publishing Foundation},
  issn = {1942-9681},
  doi = {10.1037/tra0000295},
  abstract = {Background: Previous studies have mainly considered war-affected youth as a homogenous group yet several subpopulations of war-affected youth, such as survivors of sexual violence, exist with unique mental health problems and treatment needs. This study aimed to assess posttraumatic stress disorder (PTSD), perceptions and meaning of mental illness, and access and barriers to mental health care among survivors of sexual violence. Method: Data were collected from survivors of sexual violence during war (N = 181) who are participants in the longitudinal War-Affected Youth Survey (WAYS) study in Northern Uganda. Chi-square tests of independence and binary logistic regression were used to compute participants' characteristics and assess relations between exposure to sexual violence and PTSD. Results: Sixty-six (n = 119, 66\%) reported sexual abuse: 35\% (n = 63) of whom returned from captivity with at least 1 child, and 43\% (n = 78) met the criteria for PTSD (Impact of Events Scale---Revised score [IES-R] {$\geq$}33). Those who reported sexual abuse scored significantly higher on PTSD (OR = 3.23; 95\% CI [2.09, 6.93]), perceived more stigma, reported more barriers to seeking care, and viewed mental illness as futile and fatal compared with their peers without a history of sexual abuse. Conclusions: Survivors of sexual violence are at risk of PTSD and report major obstacles to treatment and care. More resources should be allocated for interventions to improve access to care for survivors of sexual violence. Psychoeducation to create awareness, demystify myths and public stigma about mental illness, and trauma-focused cognitive-behavioral therapies to reduce PTSD among survivors are recommended. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
  keywords = {*War Exposure,Adult,Chi Square Test,Cognition,Cognitive Therapy -- Methods,Cross-Sectional Studies,Female,Follow-Up Studies,Health Service Needs,Health Services Accessibility,Human,Humans,Logistic Regression,Longitudinal Studies,Mental Disorders,Mental Disorders -- Therapy,Mental Disorders/*epidemiology/*therapy,Mental Health Services,mental illness,Northern Uganda,Odds Ratio,Posttraumatic Stress Disorder,Prospective Studies,Psychiatric Care,Psychoeducation,PTSD,Scales,services,Sex Offenses,Sex Offenses/*psychology,Sexual Abuse -- Uganda,sexual violence,Social Stigma,Stigma,Stress Disorders Post-Traumatic -- Prevention and Control,Stress Disorders Post-Traumatic -- Risk Factors,Stress Disorders Post-Traumatic -- Therapy,Surveys,Survivors,Survivors -- Psychosocial Factors,Survivors/*psychology,Test Construction,Treatment Barriers,Uganda,Uganda/epidemiology,Violence,War,Young Adult}
