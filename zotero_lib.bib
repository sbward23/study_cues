@article{starfeltQualitativeExplorationYoung2015,
  title = {A Qualitative Exploration of Young {{Australian}} Adults' Understanding of and Explanations for Alcohol-Involved Rape},
  author = {Starfelt, Louise Charlotte and Young, Ross McD. and Palk, Gavan and White, Katherine M.},
  year = {2015},
