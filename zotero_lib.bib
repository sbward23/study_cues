  pmcid = {PMC5098269},
  pmid = {27739384},
  keywords = {Comorbidity,Humans,Mental Disorders,Models Psychological},
  file = {/Users/sbward/Zotero/storage/CGL9NUNG/Forbes et al. - 2016 - Beyond comorbidity Toward a dimensional and hiera.pdf}
}

