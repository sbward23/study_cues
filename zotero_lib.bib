@article{wilhiteLongitudinalEventLevelInvestigation2018,
  title = {A {{Longitudinal Event-Level Investigation}} of {{Alcohol Intoxication}}, {{Alcohol-Related Blackouts}}, {{Childhood Sexual Abuse}}, and {{Sexual Victimization}} among {{College Students}}},
  author = {Wilhite, Emily R. and Mallard, Travis and Fromme, Kim},
