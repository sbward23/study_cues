@phdthesis{santanaImpactReligiosityAlcohol2019,
  title = {The Impact of Religiosity on Alcohol Expectancies},
  author = {Santana, Romeo},
  year = {2019},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {80},
  number = {3-B(E)},
  abstract = {Alcohol abuse and dependency is a problem that has affected the entire world for centuries. The current literature has shown a relationship between alcohol expectancies and alcohol use and has also found that religion can serve as a protective factor against excessive alcohol use. The current study therefore investigated whether religiosity has an effect on alcohol expectancies, whether individuals of different religious affiliations report different levels of alcohol use, whether alcohol expectancies are associated with quantity and frequency of alcohol use, and whether alcohol expectancies mediate the relationship between both religiosity and alcohol use. The sample consisted of 158 adults, between the ages of 21--50, who completed an online survey on Amazon Mechanical Turk. The online survey consisted of a demographic questionnaire, RCI-10, DDQ-R, and the Comprehensive Effects of Alcohol (CEOA) questionnaire. Results indicated that religiosity was not significantly associated with alcohol expectancies. In terms of religious affiliation, participants who identified as Muslims had the lowest alcohol use (both in frequency and quantity) when compared to individuals who identified with another religion. Higher levels of positive expectancies were correlated with greater drinking quantity and frequency. However, it was found that higher negative expectancies were associated with greater drinking frequency. Finally, the results of this study found that alcohol expectancies did not mediate the relationships between religiosity and alcohol use. As a result, understanding specific religious practices may assist substance abuse prevention and intervention program developers and mental health practitioners to develop faith-based interventions and practices to prevent and address problems of excessive alcohol use with their adherents. These initiatives will benefit the religious community, by preventing problems caused by excessive drinking, such as social stigma, physical and mental health problems, violent behavior, and risky sexual behavior. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {Alcohol Abuse,alcohol expectancies,Intervention,intervention programs,religiosity,Religiosity}
}

@article{santelliDoesSexEducation2018,
  title = {Does Sex Education before College Protect Students from Sexual Assault in College?},
  author = {Santelli, John S. and Grilo, Stephanie A. and Choo, Tse-Hwei and Diaz, Gloria and Walsh, Kate and Wall, Melanie and Hirsch, Jennifer S. and Wilson, Patrick A. and Gilbert, Louisa and Khan, Shamus and Mellins, Claude A.},
  year = {2018},
  month = nov,
  journal = {PLOS ONE},
  volume = {13},
  number = {11},
  pages = {e0205951},
  publisher = {Public Library of Science},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0205951},
  urldate = {2021-09-03},
  abstract = {Purpose College-bound young people experience sexual assault, both before and after they enter college. This study examines historical risk factors (experiences and exposures that occurred prior to college) for penetrative sexual assault (PSA) victimization since entering college. Methods A cross-sectional study, including an online population-based quantitiative survey with undergraduate students was conducted in spring 2016. Bivariate analyses and multivariable regressions examined risk and protective factors associated with ever experiencing PSA since entering college. Concurrently-collected in-depth ethnographic interviews with 151 students were reviewed for information related to factors identified in the survey. Results In bivariate analyses, multiple historical factors were significantly associated with PSA in college including adverse childhood experiences and having experienced unwanted sexual contact before college (for women) and initiation of alcohol, marijuana, and sexual behaviors before age 18. Significant independent risk factors for college PSA included female gender, experiencing unwanted sexual contact before college, first oral sex before age 18, and ``hooking up'' (e.g., causual sex or sex outside a committed partnership) in high school. Receipt of school-based sex education promoting refusal skills before age 18 was an independent protective factor; abstinence-only instruction was not. In the ethnographic interviews, students reported variable experiences with sex education before college; many reported it was awkward and poorly delivered. Conclusions Multiple experiences and exposures prior to college influenced the risk of penetrative sexual assault in college. Pre-college comprehensive sexuality education, including skills-based training in refusing unwanted sex, may be an effective strategy for preventing sexual assault in college. Sexual assault prevention needs to begin earlier; successful prevention before college should complement prevention efforts once students enter college.},
  langid = {english},
  keywords = {Adolescents,Human families,Human sexual behavior,Medical risk factors,Rape and sexual assault,Religion,Schools,Undergraduates},
  file = {/Users/sbward/Zotero/storage/9FZJDBLY/Santelli et al. - 2018 - Does sex education before college protect students.pdf;/Users/sbward/Zotero/storage/5XTM2AEY/article.html}
}

@article{sarkhelAdjunctiveHighfrequencyRight2010,
  title = {Adjunctive High-Frequency Right Prefrontal Repetitive Transcranial Magnetic Stimulation ({{rTMS}}) Was Not Effective in Obsessive--Compulsive Disorder but Improved Secondary Depression},
  author = {Sarkhel, Sujit and Sinha, Vinod Kumar and Praharaj, Samir Kumar},
  year = {2010},
  month = jun,
  journal = {Journal of Anxiety Disorders},
  volume = {24},
  number = {5},
  pages = {535--539},
  publisher = {Elsevier Science},
  issn = {0887-6185},
  doi = {10.1016/j.janxdis.2010.03.011},
  abstract = {Background: There is preliminary evidence that repetitive transcranial magnetic stimulation (rTMS) may be useful in obsessive--compulsive disorder (OCD) patients. Methods: Our objective was to examine efficacy of adjunctive right prefrontal high-frequency (rapid) rTMS treatment in OCD patients. 42 patients with OCD were randomly assigned to 10 sessions of addon high-frequency right prefrontal active rTMS (10 Hz, 110\% of motor threshold, 4 s per train, 20 trains per session) or sham stimulation. They were rated on Yale Brown Obsessive Compulsive Scale (YBOCS), Hamilton Rating Scale for Depression (HAM-D), Hamilton Rating Scale for Anxiety (HAM-A) and Clinical Global Impression-Severity of Illness (CGI-S) at baseline, day 14 and day 28. The dose of antiobsessive drug was kept constant throughout the period of assessment. Results: For YBOCS scores, repeated measures ANOVA showed significant main effect of treatment, but no effect of treatment over time (Pillai's Trace F = 1.39, p = .262). However, significant effect of treatment over time as shown by interaction effect for both HAM-D (Pillai's Trace F = 3.67, p = .035, {$\eta^2$} = .158) and HAM-A scores (Pillai's Trace F = 5.22, p = .01, {$\eta^2$} = .211) were seen. Conclusion: Adjunctive high-frequency right prefrontal rTMS does not have any significant effect in the treatment of OCD. However, it is modestly effective in the treatment of comorbid depressive symptoms in patients with OCD. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
  keywords = {adjunctive treatment,Adjunctive Treatment,Adolescent,Adult,Depressive Disorder Major,Female,Humans,International Classification of Diseases,Major Depression,Male,Middle Aged,obsessive compulsive disorder,Obsessive Compulsive Disorder,Obsessive-Compulsive Disorder,Prefrontal Cortex,Prospective Studies,repetitive transcranial magnetic stimulation,secondary depression improvement,Surveys and Questionnaires,Transcranial Magnetic Stimulation,Young Adult}
}

