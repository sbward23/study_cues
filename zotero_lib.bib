  journal = {Behaviour Research and Therapy},
  volume = {45},
  number = {10},
  pages = {2432--2447},
  doi = {10.1016/j.brat.2007.05.002},
  abstract = {A randomized between-group design was used to evaluate the efficacy of a video intervention to reduce post-traumatic stress disorder (PTSD) and other mental health problems, implemented prior to the forensic medical examination conducted within 72 h post-sexual assault. Participants were 140 female victims of sexual assault (68 video/72 nonvideo) aged 15 years or older. Assessments were targeted for 6 weeks (Time 1) and 6 months (Time 2) post-assault. At Time 1, the intervention was associated with lower scores on measures of PTSD and depression among women with a prior rape history relative to scores among women with a prior rape history in the standard care condition. At Time 2, depression scores were also lower among those with a prior rape history who were in the video relative to the standard care condition. Small effects indicating higher PTSD and Beck Anxiety Inventory (BAI) scores among women without a prior rape history in the video condition were observed at Time 1. Accelerated longitudinal growth curve analysis indicated a video{\texttimes}prior rape history interaction for PTSD, yielding four patterns of symptom trajectory over time. Women with a prior rape history in the video condition generally maintained the lowest level of symptoms.},
  keywords = {Depression,Early intervention,PTSD,Rape,Video}
}

@phdthesis{resnickRorschachProfilesTraumatized2002,
  title = {Rorschach Profiles of Traumatized Inner-City Children with and without {{PTSD}}},
  author = {Resnick, Justin David},
  year = {2002},
  month = may,
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {62},
  number = {11-B},
  pages = {5388},
  abstract = {Posttraumatic stress disorder (PTSD) in children causes wide-ranging detriment to children's emotional, social, and academic development. PTSD can be difficult to diagnose in children, with symptoms resembling attention deficits, depression, anxiety, oppositional-defiant disorder, or attachment disorder. If not recognized and treated promptly, studies suggest that PTSD can become an entrenched chronic condition which is highly resistant to treatment. Unfortunately, self-report methods for identifying traumatized children may be hindered by the very symptoms and dynamics that PTSD creates (e.g., avoiding recollection of the trauma, feeling ashamed, self-blame, distrusting others, etc.). The present study examined whether a projective method, the Rorschach, might be useful in assessing PTSD among inner-city children suspected for trauma. The present study examined three core features of PTSD symptomology-psychological constriction, flooding, and dissociation-observed in Rorschachs of persons with PTSD. Using composites of Comprehensive System variables, Rorschach scales were developed for measuring Constriction, Flooding, and Dissociation, to determine whether these dimensions differentiate PTSD from Non-PTSD among inner-city children exposed to potential traumas (community violence, physical or sexual abuse, domestic violence). Subjects were drawn from two archival sources, one with children referred for suspected maltreatment (N = 55), and another with inner-city schoolchildren who self-reported trauma symptoms on a paper-and-pencil survey (N =17). Convergent evidence from the two samples supported the relationship between PTSD symptomology and Rorschach Dissociation, and between PTSD and Rorschach Flooding. The Constriction scale showed no consistent relationship with PTSD, although both samples were profoundly constricted regardless of trauma status. Elevations on Dissociation appeared frequently among PTSD and subthreshold PTSD children but were rare among nontraumatized children, as expected. This Rorschach Dissociation scale still requires validation against established measures of child dissociation. Flooding scores correlated consistently with higher PTSD symptomology, but unexpectedly, Flooding elevations were nearly absent among children with diagnosable PTSD. This unexpected finding needs to be replicated. The lack of Flooding in PTSD children may reflect their avoidance of trauma-related distress, and thus may signal interruption of the healing process. Future research could evaluate whether elevated Rorschach Flooding predicts better outcomes, reflecting a child actively 'working through' trauma versus being trapped in PTSD-related avoidance. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {Posttraumatic Stress Disorder,PTSD: inner city children,Rorschach,Rorschach Test,trauma,Urban Environments}
}

