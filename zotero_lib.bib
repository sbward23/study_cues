@article{levi-belzMoralInjuryPredeployment2024,
  title = {Moral Injury and Pre-Deployment Personality Factors as Contributors to Psychiatric Symptomatology among Combatants: A Two-Year Prospective Study},
  shorttitle = {Moral Injury and Pre-Deployment Personality Factors as Contributors to Psychiatric Symptomatology among Combatants},
  author = {{Levi-Belz}, Yossi and {Ben-Yehuda}, Ariel and Levinstein, Yoav and Zerach, Gadi},
  year = {2024},
