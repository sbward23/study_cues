@phdthesis{groffRelationshipSocialConstraints2016,
  title = {The Relationship between Social Constraints, Social Support, Loss-Related Factors, Depression, and {{PTSD}} in a Treatment-Seeking Suicide Bereaved Sample},
  author = {Groff, Elizabeth C.},
  year = {2016},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {77},
  number = {4-B(E)},
  abstract = {Those bereaved by suicide face unique interpersonal and social demands. Stigma, blame, uncertainty, and social discomfort can pose obstacles to social support. According to the Social Cognitive Processing model of adjustment to trauma (Lepore, 2001), adjustment is facilitated by talking with supportive others who help the trauma survivor express, understand, and come to terms with their trauma-related thoughts and feelings. Perceived or actual negative social responses may cause the bereaved to feel unsupported, misunderstood, or alienated from their social network. The adverse effects of such social constraints have been studied in non-bereaved medical patients and survivors of sexual assault and physical violence. However, social constraints have not been as thoroughly evaluated in bereaved trauma survivors and research in suicide bereavement is even more limited. The purpose of this study was to examine relationships among social constraints, social support, loss-related factors (e.g., kinship, exposure to the body), depression, and PTSD in a treatment-seeking sample of civilians recently bereaved by suicide. Data was drawn from pre-treatment, baseline data from a graduate school-based research clinic; only data from suicide bereaved participants was used. Baseline assessments included the PTSD Checklist-Civilian (PCL-C), the Center for Epidemiologic Study-Depression Scale (CES-D), the Social Constraints Scale (SCS), and the Duke-UNC Functional Social Support Questionnaire (DUKE-SSQ). Items that were the highest rated and most frequently rated 'often' were: Others did not seem to understand your situation, Others acted uncomfortable when you talked about the event or its effects, Others seem to be hiding their feelings, Others make you feel as though you had to keep your feelings to yourself because they made them feel uncomfortable, Others tell you to try not to think about the event, and Others give you the idea that they didn't want to hear about the event. Social constraints were negatively associated with social support and positively associated with PTSD and depression. Kinship was significantly associated with depression, but not social constraints or PTSD; closer kinship was associated with lower depressive symptoms. Finding the body was not significantly associated with social constraints or distress. Findings suggest that social constraints are closely linked with greater distress and should be considered in suicide postvention. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {depression,loss-related factors,Major Depression,Posttraumatic Stress Disorder,PTSD,social constraints,social support,Social Support,suicide,Treatment,treatment-seeking}
}

@article{gronertUsingPopularMedia2022,
  title = {Using Popular Media Portrayals to Investigate Undergraduates' Perceptions of Sexual Consent},
  author = {Gronert, Nona Maria},
  year = {2022},
  month = aug,
  journal = {Gender and Education},
  volume = {34},
  number = {6},
  pages = {643--658},
  publisher = {Routledge},
  issn = {0954-0253},
  doi = {10.1080/09540253.2022.2027887},
  urldate = {2024-01-16},
  abstract = {Sexual assault is sexual activity without consent, but what is consent? Undergraduates' understandings are often messier than the consensual/non-consensual binary. U.S. university undergraduates watched videos depicting consensual sexual interactions and sexual assaults. Scenes were drawn from popular television programmes and pornographic websites, and varied in their inclusions of verbal consent, alcohol consumption, and coercion. The results show promise for the method of using popular media vignettes to investigate undergraduates' consent perceptions. Findings illustrate that women who never viewed pornography found the scenes less consensual than men who frequently viewed pornography. Despite U.S. law requiring universities to provide sexual consent education to incoming students, such sexuality education had only limited effects on perceptions of consent. Findings suggest that semester-long educational courses were more effective than one-time trainings in impacting perceptions of consent.},
  keywords = {affirmative consent,higher education,sexual assault,Sexual consent,vignette}
}

@article{groseSexualEducationGender2014,
  title = {Sexual {{Education}}, {{Gender Ideology}}, and {{Youth Sexual Empowerment}}},
  author = {Grose, Rose Grace and Grabe, Shelly and Kohfeldt, Danielle},
  year = {2014},
  month = oct,
  journal = {The Journal of Sex Research},
  volume = {51},
  number = {7},
  pages = {742--753},
  publisher = {Taylor \& Francis},
  issn = {0022-4499},
  doi = {10.1080/00224499.2013.809511},
  urldate = {2023-06-26},
  abstract = {Sexual education plays an essential role in preventing unplanned pregnancy and the transmission of sexually transmitted infections (STIs). School-based sexual education programs, in particular, may be well positioned to address social factors that are empirically linked to negative sexual health outcomes, such as traditional social norms surrounding gender and sexuality. However, youth are seldom granted access to sexual education programs that explicitly address these issues. This study presents findings from a pretest--posttest survey of a sexual education program that did. It was designed for eighth graders (N=95) in the context of a school--community collaboration. The study assessed the links between several components of sexual empowerment, including gender ideology, sexual knowledge, and contraceptive beliefs. Findings link participation in the sexual education program to more progressive attitudes toward girls and women, less agreement with hegemonic masculinity ideology, and increases in sexual health and resource knowledge. Structural equation models suggest that traditional attitudes toward women were significantly related to hegemonic masculinity ideology among both boys and girls, which was in turn negatively related to safer contraceptive beliefs.},
  pmid = {24024546}
}

