@article{bhochhibhoyaRiskFactorsSexual2019,
  title = {Risk {{Factors}} for {{Sexual Violence Among College Students}} in {{Dating Relationships}}: {{An Ecological Approach}}},
  shorttitle = {Risk {{Factors}} for {{Sexual Violence Among College Students}} in {{Dating Relationships}}},
