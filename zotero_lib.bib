@phdthesis{villarrealLatinaWomensReasons2012,
  title = {Latina Women's Reasons for and Experience of Sexual Assault Disclosure: {{A}} Qualitative Investigation},
  author = {Villarreal, Melissa},
  year = {2012},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {73},
  number = {1-B},
  pages = {271},
  abstract = {This study describes Latina women's experiences of sexual assault disclosure. The research questions are: (1) What reasons do Latinas give for choosing to disclose, not to disclose, or to delay disclosure of their experiences of sexual assault? (2) Can patterns be detected regarding to whom Latinas disclose and under what circumstances? (3) What feelings and consequences do Latinas remember experiencing during their initial disclosure? (4) In retrospect, how do Latinas interpret their self-disclosure decisions? Participants for this study agreed to be interviewed about their sexual assault experiences. Seven interviews were gathered in 2005 and six in 2011. A constant comparison approach was used with the transcribed interviews to discern recurrent significant themes. Similar to earlier research, all 13 participants had been sexually assaulted as children; most were assaulted by known perpetrators; and those who delayed disclosure cited reasons such as fear, family shame, avoidance, and self-blame. Furthermore, as in earlier research, taboos on talking about sex limited the awareness Latinas had with regard to sexual assault. In this study, some Latinas identified ignorance as a reason for not disclosing or delaying disclosure. Many of the women in this study identified complicated mother and daughter relationship dynamics that prevented disclosures. Similar to earlier research, participants more frequently disclosed to family members or friends for emotional support rather than to professionals. Some Latinas disclosed for protection of self and others. Some reported feeling spoiled for marriage, silenced, and disbelieved. This study adds information to the literature about feelings experienced during the initial disclosure---regret, shame, and negative judgment of self. Many of the women in this study demonstrated that they had transcended the trauma of their sexual assault. This provides direct evidence confirming assumptions from prior research in which psychotherapists were the interviewees. This study contributes Latinas' voices to the literature on sexual assault disclosure. It points to the need for further research on social services interventions to empower Latina sexual assault survivors. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {decision making,Decision Making,family members,Family Members,Human Females,Latinos,Latinos/Latinas,life experiences,Life Experiences,self disclosure,Self-Disclosure,Sex Offenses,sexual assaults,women}
}

@phdthesis{vithalPrefrontalCortexModulation2020,
  title = {Prefrontal Cortex Modulation Using Transcranial Direct Current Stimulation in Alcohol Craving: {{A}} Sham-Controlled Double Blind Study},
  author = {Vithal, Ghodke Jayesh},
  year = {2020},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {81},
  number = {5-B},
  abstract = {Background: Functional neuroimaging studies have shown that specific brain areas are associated with alcohol craving including the dorsolateral prefrontal cortex (DLPFC). Transcranial direct current stimulation (tDCS) is a form of non-invasive brain stimulation technique that is increasingly being utilised to examine and modify a number of cognitive and behavioural measures. tDCS has been shown to reduce acute substance craving in smoking and alcohol dependence and improve cognition in neuropsychiatric patients. Aim: The present study intended to study the efficacy of tDCS of the left dorsolateral pre-frontal cortex and the right dorsolateral pre-frontal cortex in reducing craving in patients with alcohol dependence. Method: A randomized sham-controlled study in which 60 subjects received ten sessions each of sham and active bilateral tDCS delivered to DLPFC was performed. Of these, 20 received left active (anodal left/cathodal right) and 20 received right active (anodal right/cathodal left) tDCS. For sham stimulation, the electrodes were placed at the same positions as in active stimulation; however, the stimulator was turned off after 30 s of stimulation. Subjects were rated before, after the ten sessions, after two weeks and after four weeks for alcohol craving with the Alcohol Craving Questionnaire (ACQ-NOW). Results: The results showed that repetitive anodal left/cathodal right and anodal right/cathodal left significantly decreased alcohol craving as compared to sham stimulation (p {$<$} 0.0001) with no significant difference between left active and right active stimulation (p {$<$} 0.0001). In addition, we found that following treatment, effect on craving could not be sustained over time. Conclusions: The findings showed that repetitive tDCS treatment to DLPFC has short-term anti-craving effects. These findings extend the results of previous studies using tDCS to reduce craving in humans. With the relatively rapid suppressive effect of tDCS on and the unpredictable nature of alcohol craving, this technique may prove to be a valuable treatment strategy within the clinical setting. (PsycINFO Database Record (c) 2020 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {Alcohol craving,Behavior Modification,Cognitive Assessment,Craving,Dorsolateral prefrontal cortex,Dorsolateral Prefrontal Cortex,Electrical Brain Stimulation,Neuroimaging,Prefrontal Cortex,Transcranial direct current stimulation,Transcranial Direct Current Stimulation}
}

