@article{abebeClusterrandomizedTrialMiddle2017,
  title = {A Cluster-Randomized Trial of a Middle School Gender Violence Prevention Program: {{Design}}, Rationale, and Sample Characteristics},
  shorttitle = {A Cluster-Randomized Trial of a Middle School Gender Violence Prevention Program},
  author = {Abebe, Kaleab Z. and Jones, Kelley A. and Ciaravino, Samantha and Ripper, Lisa and Paglisotti, Taylor and Morrow, Sarah Elizabeth and Grafals, Melanie and Van Dusen, Courtney and Miller, Elizabeth},
