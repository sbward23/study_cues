@article{oesterleImpulsivityReactiveProactiveAggression2023,
  title = {Impulsivity and {{Reactive-Proactive Aggression}} as {{Mechanisms}} of {{Alcohol-Related Sexual Intimate Partner Violence Perpetration}} among {{Romantic Partners}}},
  author = {Oesterle, Daniel W. and Eckhardt, Christopher I. and Parrott, Dominic J.},
