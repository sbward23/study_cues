@article{meadeIdentifyingCarelessResponses2012,
  title = {Identifying Careless Responses in Survey Data},
  author = {Meade, Adam W. and Craig, S. Bartholomew},
  year = {2012},
  month = sep,
  journal = {Psychological Methods},
  volume = {17},
