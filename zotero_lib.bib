@article{trocheRoleDominanceNurturance2017,
  title = {On the Role of Dominance and Nurturance in the Confluence Model: {{A}} Person-Centered Approach to the Prediction of Sexual Aggression},
  shorttitle = {On the Role of Dominance and Nurturance in the Confluence Model},
  author = {Troche, Stefan J. and Herzberg, Philipp Yorck},
  year = {2017},
  journal = {Aggressive Behavior},
