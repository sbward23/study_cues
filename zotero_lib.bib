  doi = {10.1089/caff.2018.0004},
  urldate = {2021-09-03},
  abstract = {Background: Alcohol mixed with energy drink (AmED) use among college students is associated with specific adverse effects (e.g., sexual assault, driving while intoxicated). This study offered the opportunity to evaluate correlates of past year AmED use in a large sample of college students., Methods: In autumn 2010, emails were sent to all first year students (n\,=\,29,536) at 11 colleges and universities in North Carolina and Virginia, inviting them to participate in a brief web-based survey to be used to establish a cohort for the parent tobacco use study; 10,340 (35\%) students responded to the web-based survey, which elicited items on demographics, past year use of AmED, and other health behaviors., Results: Past year of AmED was reported by 29.4\% of students. Logistic regression analysis revealed that female gender (adjusted odds ratios [AOR]\,=\,1.28, 95\% confidence interval [CI]\,=\,1.16--1.42); ever use of smokeless tobacco (AOR\,=\,3.51, 95\% CI\,=\,3.00--4.10); current smoking (AOR\,=\,4.97, 95\% CI\,=\,4.34--5.69); 2 or fewer days of physical activity per week (AOR\,=\,1.16, 95\% CI\,=\,1.04--1.28); and 7 or 8 hours of sleep per day (AOR\,=\,1.12, 95\% CI\,=\,1.02--1.24) predicted risk of past year use of AmED., Conclusions: Nearly a third of first year college students reported past year use of AmED. Our finding of increased past year use of AmED use among women is new and might reflect the changing epidemiology of female college alcohol use. Of additional concern, such use might also reflect targeted marketing to women by the energy drink industry. Findings can guide future research as well as substance counseling to college students.},
  pmcid = {PMC6150939},
  pmid = {30250945},
  file = {/Users/sbward/Zotero/storage/VQWH3ZR4/Spangler et al. - 2018 - Correlates of Alcohol Mixed with Energy Drink Use .pdf}
}

@phdthesis{spauldingDomesticViolenceDisclosure2021,
  title = {Domestic Violence Disclosure Decisions of {{Latina}} Immigrants: {{A}} Phenomenological Study},
  author = {Spaulding, DeAnza J.},
  year = {2021},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {82},
  number = {3-B},
  abstract = {The phenomenological research was conducted to explore the lived experience of Latina immigrant survivors of domestic violence; Latina's decision to disclose domestic violence and receive assistance; the mitigating factors of acculturation on their decision; and recommendations for culturally competent approaches in support services from the Latina immigrant perspective. Domestic violence researchers have focalized study on Western models to explore domestic abuse. Immigrants faced challenges accessing resources and assistance due to a lack of culturally competent services. Studies showed that the harmful psychological impact of domestic abuse are mitigated with social, legal and mental health support. Purposeful sampling was used to conduct qualitative, semi-structured, face-to-face interviews with eight Latina survivors of domestic violence. The research interviews yielded data descriptions of trauma and stress due to domestic violence, social discrimination and the immigration process. Results found that all of the participants dealt with challenges to accessing services due to stigma; difficulty finding support services in their communities; psychological trauma from domestic abuse; and dealing with an uncertain, indeterminate immigration process. The culturally relevant services that Latinas found helpful were talking with Spanish speaking advocates and culturally competent and non-judgmental service providers. Service providers who understood the survivors' cultural context inspired trust and linked survivors to resources for housing, legal advocacy and psychological care. Participants stated that resources that were implemented with cultural sensitivity benefited their safety goals and emotional well-being. Participants suggested outreach to Latina survivors needed to include: building relationships and engaging community prevention in their neighborhoods. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {domestic violence,Domestic Violence,Immigration,Interviews,Latina immigrants,Latinos/Latinas,mental health,Mental Health,Phenomenology,survivors,Survivors,Trauma}
}

