}

@article{hustViewersPerceptionsObjectified2019,
  title = {Viewers' {{Perceptions}} of {{Objectified Images}} of {{Women}} in {{Alcohol Advertisements}} and {{Their Intentions}} to {{Intervene}} in {{Alcohol-Facilitated Sexual Assault Situations}}},
