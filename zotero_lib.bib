@article{badourDisgustMentalContamination2013,
  title = {Disgust, Mental Contamination, and Posttraumatic Stress: {{Unique}} Relations Following Sexual versus Non-Sexual Assault},
  shorttitle = {Disgust, Mental Contamination, and Posttraumatic Stress},
  author = {Badour, Christal L. and Feldner, Matthew T. and Babson, Kimberly A. and Blumenthal, Heidemarie and Dutton, Courtney E.},
