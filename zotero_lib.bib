@article{jeffreyIntersectionsNormativeHeterosexuality2020,
  title = {The {{Intersections}} of {{Normative Heterosexuality}} and {{Sexual Violence}}: {{University Men}}'s {{Talk}} about {{Sexual Behavior}} in {{Intimate Relationships}}},
