@phdthesis{fritzwilliamsYesNoMaybe2022,
  title = {Yes, {{No}}, and {{Maybe}}: {{The Creation}} of a {{Multidimensional Scale}} to {{Measure Sexual Consent Behavioral Intentions}} and the {{Exploration}} of the {{Role}} of {{Campus Sexual Health Messages}}},
