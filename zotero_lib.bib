@article{priceNeurobiologyTryptophanDepletion1997,
  title = {Neurobiology of Tryptophan Depletion in Depression: {{Effects}} of m-Chlorophenylpiperazine ({{mCPP}})},
  author = {Price, Lawrence H. and Malison, Robert T. and McDougle, Christopher J. and {McCance-Katz}, Elinore F. and Owen, Kim R. and Heninger, George R.},
  year = {1997},
  month = nov,
  journal = {Neuropsychopharmacology},
  volume = {17},
  number = {5},
  pages = {342--350},
  publisher = {Nature Publishing Group},
  issn = {0893-133X},
  doi = {10.1016/S0893-133X(97)00084-5},
  abstract = {Utilized neuroendocrine and mood responses to iv infusion of the serotonin (5-HT) agonist m-chlorophenylpiperazine (mCPP) to evaluate central 5-HT function in depressed patients undergoing acute tryptophan (TRP) depletion. 22 drug-free patients (aged 29--66 yrs) with major depression participated. Each patient underwent 2 randomized, double-blind TRP depletion tests, one sham and one active. At the estimated time of maximum TRP depletion, each patient received an iv infusion of mCPP 0.1 mg/kg. Blood was obtained for serum cortisol, prolactin, and growth hormone. Multiple rating scales were used to assess mood. The cortisol response to iv mCPP was significantly greater during TRP depletion than during sham depletion, and free plasma TRP was negatively correlated with the cortisol response during TRP depletion. These findings are consistent with the hypothesis that acute TRP depletion in drug-free depressed patients induces a compensatory up-regulation of postsynaptic 5-HT receptors, most likely of the 5-HT2A/2C subtype. Such changes suggest a mechanism by which acute and potent manipulations of 5-HT function in depressed patients could be used to effect rapid clinical improvement. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {29-66 yr old drug-free patients with major depression,Adult,Affect,Aged,Amino Acids,central serotonin function,Depressive Disorder,Double-Blind Method,Female,Human Growth Hormone,Humans,Hydrocortisone,Infusions Intravenous,iv infusion of m-chlorophenylpiperazine during acute tryptophan depletion,Major Depression,Male,Middle Aged,Neurobiology,Patients,Piperazines,Prolactin,Serotonin,Serotonin Agonists,Serotonin Receptor Agonists,Tryptophan}
}

@article{priceNeurobiologyTryptophanDepletion1998,
  title = {The Neurobiology of Tryptophan Depletion in Depression: {{Effects}} of Intravenous Tryptophan Infusion},
  author = {Price, Lawrence H. and Malison, Robert T. and McDougle, Christopher J. and Pelton, Gregory H. and Heninger, George R.},
  year = {1998},
