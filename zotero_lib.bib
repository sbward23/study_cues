@article{robertsSexualBehaviorSubstance2021,
  title = {Sexual Behavior and Substance Use in Adolescence and Young Adulthood: {{Non-specific}} Associations between a Range of Sexual Behaviors and Alcohol, Nicotine, and Marijuana Use},
  shorttitle = {Sexual Behavior and Substance Use in Adolescence and Young Adulthood},
  author = {Roberts, Hannah and Clark, D. Angus and Kalina, Claire and Sherman, Carter and Heitzeg, Mary M. and Hicks, Brian M.},
  year = {2021},
