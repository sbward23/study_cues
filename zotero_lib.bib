  author = {Spangler, John G. and Song, Euyoung Y. and Egan, Kathleen L. and Wagoner, Kimberly G. and Reboussin, Beth A. and Wolfson, Mark and Sutfin, Erin L.},
  year = {2018},
  month = sep,
  journal = {Journal of Caffeine and Adenosine Research},
