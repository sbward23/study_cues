@article{vanlankveldIndirectMeasureImplicit2022,
  title = {An {{Indirect Measure}} of {{Implicit Sexual Assertiveness}}: {{Reliability}} and {{Validity}} of the {{Sexual Assertiveness IAT}} in {{Young Adults}} in {{The Netherlands}}},
