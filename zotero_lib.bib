}

@phdthesis{distelDisclosureChildhoodSexual1999,
  title = {Disclosure of Childhood Sexual Abuse: {{Links}} to Emotion Expression and Adult Attachment},
  author = {Distel, Nancy E.},
  year = {1999},
  month = jan,
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {60},
  number = {6-B},
  pages = {2938},
  abstract = {This study examined several factors related to the disclosure patterns of childhood sexual abuse survivors. The time until disclosure and the victim's choice of initial disclosure recipient were associated with aspects of the victim-perpetrator relationship. In addition, the survivors' self-reported childhood emotion coping strategies were related to disclosure. Furthermore, survivors' adult attachment dimension profiles, used as indicators of long-term intimate functioning, were related to the other variables. Finally, this study explored patterns of self-reported emotional responses to abuse. Forty-five adult, female survivors of childhood sexual abuse completed a series of self-report measures. All of the women had disclosed the abuse prior to participation. Measures included a background questionnaire, portions of the Emotions as a Child Questionnaire, the Bartholomew and Horowitz Relationship Scales Questionnaire, and the Sex Abuse Questionnaire. Results indicated that the closer the victim-perpetrator relationship, the longer the survivor waited to disclose the abuse. Earlier disclosures were associated with extrafamilial perpetrators, childhood emotional expressiveness, and disclosures to immediate family. Later disclosures were associated with incest, withdrawal and distraction emotion coping strategies, disclosure to persons outside the immediate family, and self-reported dismissing-avoidant attachment behaviors. Further, survivors whose responses loaded on the avoidant attachment prototypes tended to expect less support upon disclosure and to disclose to persons other than immediate family. In contrast, more securely attached survivors were more likely to have disclosed sooner and to immediate family. Finally, exploratory analysis of the victim's self-reported emotional responses to the abuse revealed high levels of experienced fear, sadness, and shame, as well as elevations in surprise, guilt, and anger. Analyses of the impact of perpetrator threat on time until disclosure did not reach significance. In addition, no significant differences were evidenced between incest and non-incest survivors with regard to chosen disclosure recipient. The present findings highlight the complexity of the disclosure process following childhood sexual abuse while demonstrating the usefulness of applying theories of emotion and attachment to this process. The need for further methodological developments and research of sexual abuse in the context of these theories is addressed. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {adult female survivors of childhood sexual abuse,aspects of victim-perpetrator relationship & adult attachment & emotional coping strategies,Attachment Behavior,Child Abuse,Coping Behavior,disclosure patterns & emotional response to abuse,Human Females,Self-Disclosure,Sexual Abuse}
}

@article{ditzerChildMaltreatmentAlexithymia2023,
  title = {Child Maltreatment and Alexithymia: {{A}} Meta-Analytic Review},
  shorttitle = {Child Maltreatment and Alexithymia},
  author = {Ditzer, Julia and Wong, Eileen Y. and Modi, Rhea N. and Behnke, Maciej and Gross, James J. and Talmon, Anat},
  year = {2023},
  journal = {Psychological Bulletin},
  volume = {149},
  number = {5-6},
  pages = {311--329},
  publisher = {American Psychological Association},
  address = {US},
  issn = {1939-1455},
  doi = {10.1037/bul0000391},
  abstract = {Alexithymia refers to difficulties identifying and describing one's emotions. Growing evidence suggests that alexithymia is a key transdiagnostic risk factor. Despite its clinical importance, the etiology of alexithymia is largely unknown. The present study employs meta-analytic methods to summarize findings on the role of one hypothesized antecedent of adult alexithymia, namely child maltreatment. We obtained effect size estimates from 99 independent samples reported in 78 unique sources that reported both child maltreatment history and adult levels of alexithymia. These studies involved a total of 36,141 participants. Using correlation coefficients as our effect size index, we found that child maltreatment was positively related to overall adult alexithymia (r = .23 [.19, .27]). Notably, emotional abuse (r = .18 [.13, .23]), emotional neglect (r = .21 [.16, .26]), and physical neglect (r = .18 [.15, .22]) were the strongest predictors. Effects were moderated by gender, affiliation with clinical versus nonclinical samples, and publication status. Overall results were robust to publication bias and the presence of outliers. These findings contribute to a more nuanced understanding of the complex connection between different types of child maltreatment and alexithymia, providing greater insight into the early environmental influences on alexithymia. (PsycInfo Database Record (c) 2023 APA, all rights reserved)},
  keywords = {Alexithymia,Child Abuse,Emotional Abuse,Physical Abuse,Sexual Abuse},
  file = {/Users/sbward/Zotero/storage/X8QBTHUB/Ditzer et al. - 2023 - Child maltreatment and alexithymia A meta-analyti.pdf}
}

@article{dixonInteroceptiveAwarenessFemale2024,
  title = {Interoceptive {{Awareness}} and {{Female Orgasm Frequency}} and {{Satisfaction}}},
  author = {Dixon, Emily and Poerio, Giulia L. and Rieger, Gerulf and Klabunde, Megan},
  year = {2024},
  month = dec,
  journal = {Brain Sciences},
  volume = {14},
  number = {12},
  pages = {1236},
  publisher = {Multidisciplinary Digital Publishing Institute},
  issn = {2076-3425},
  doi = {10.3390/brainsci14121236},
  urldate = {2025-03-21},
  abstract = {Background: The female orgasm is a highly understudied phenomenon that is linked to both wellbeing and relationship satisfaction in women. Although orgasm has been associated with interoception---the sense of the physiological condition of the body---very few studies have directly examined the influence that interoception has on orgasm. Objectives: This study investigates how the subjective experience of one's interoceptive capacities (called interoceptive awareness) is associated with self-reported orgasm frequency and satisfaction in people who identify as women. Methods: In a dataset of 318 women, orgasm frequency and satisfaction were both rated significantly higher for solitary as compared to partnered sexual experiences. Results: Analysis of how dimensions of interoceptive awareness correlated with orgasm frequency and satisfaction showed that (1) `Noticing' predicted orgasm frequency (but not satisfaction) across both solitary and partnered interactions, (2) `Attention Regulation' predicted greater frequency and satisfaction of solitary orgasm (but not partnered interactions), and (3) `Body Trusting' predicted orgasm satisfaction (but not frequency) across both solitary and partnered contexts. Conclusions: Findings underscore the importance of moving beyond orgasmic dysfunction research by investigating how interoception is associated with healthy---and potentially even optimal---orgasmic functioning in women.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {interoception,orgasm,orgasm frequency,orgasm gap,orgasm satisfaction,women},
  file = {/Users/sbward/Zotero/storage/TQ9XW2J4/Dixon et al. - 2024 - Interoceptive Awareness and Female Orgasm Frequenc.pdf}
}

