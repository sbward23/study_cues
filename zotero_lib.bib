@article{talmonNullifyingExperienceSelfobjectification2016,
  title = {The Nullifying Experience of Self-Objectification: {{The}} Development and Psychometric Evaluation of the {{Self-Objectification Scale}}},
  author = {Talmon, Anat and Ginzburg, Karni},
