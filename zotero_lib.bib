  title = {Extending the {{ACEs Framework}}: {{Examining}} the {{Relations Between Childhood Abuse}} and {{Later Victimization}} and {{Perpetration With College Men}}},
  shorttitle = {Extending the {{ACEs Framework}}},
  author = {Voith, Laura A. and Anderson, RaeAnn E. and Cahill, Shawn P.},
  year = {2020},
  month = sep,
  journal = {Journal of Interpersonal Violence},
  volume = {35},
