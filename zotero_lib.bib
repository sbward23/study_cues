@article{stullVariabilityIntensivelyAssessed2022,
  title = {Variability in Intensively Assessed Mood: {{Systematic}} Sources and Factor Structure in Outpatients with Opioid Use Disorder},
  shorttitle = {Variability in Intensively Assessed Mood},
  author = {Stull, Samuel W. and Mogle, Jacqueline and Bertz, Jeremiah W. and {Burgess-Hull}, Albert J. and Panlilio, Leigh V. and Lanza, Stephanie T. and Preston, Kenzie L. and Epstein, David H.},
  year = {2022},
  month = oct,
  journal = {Psychological Assessment},
  volume = {34},
  number = {10},
  pages = {966--977},
  publisher = {American Psychological Association},
  issn = {1040-3590},
  doi = {10.1037/pas0001160},
  urldate = {2023-01-03},
  abstract = {In intensive longitudinal studies using ecological momentary assessment, mood is typically assessed by repeatedly obtaining ratings for a large set of adjectives. Summarizing and analyzing these mood data can be problematic because the reliability and factor structure of such measures have rarely been evaluated in this context, which---unlike cross-sectional studies---captures between- and within-person processes. Our study examined how mood ratings (obtained thrice daily for 8 weeks; n = 306, person moments = 39,321) systematically vary and covary in outpatients receiving medication for opioid use disorder (MOUD). We used generalizability theory to quantify several aspects of reliability, and multilevel confirmatory factor analysis (MCFA) to detect factor structures within and across people. Generalizability analyses showed that the largest proportion of systematic variance across mood items was at the person level, followed by the person-by-day interaction and the (comparatively small) person-by-moment interaction for items reflecting low arousal. The best-fitting MCFA model had a three-factor structure both at the between- and within-person levels: positive mood, negative mood, and low-arousal states (with low arousal considered as either a separate factor or a subfactor of negative mood). We conclude that (a) mood varied more between days than between moments and (b) low arousal may be worth scoring and reporting separately from positive and negative mood states, at least in a MOUD population. Our three-factor structure differs from prior analyses of mood; more work is needed to understand the extent to which it generalizes to other populations. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
  keywords = {Confirmatory Factor Analysis,Drug Therapy,ecological momentary assessment,Ecological Momentary Assessment,Emotional States,Factor Structure,generalizability theory,mood,multilevel confirmatory factor analysis,Negative Emotions,opioid use disorder,Opioid Use Disorder,Outpatients},
  file = {/Users/sbward/Zotero/storage/WXVLVGG9/Stull et al. - 2022 - Variability in intensively assessed mood Systemat.pdf}
}

@article{suschinskyRelationshipSexualConcordance2014,
