@phdthesis{matthewsSocialReactionsDisclosure2013,
  title = {Social Reactions to Disclosure, Attributions of Responsibility, and Shame as Predictors of Psychological Adjustment in Sexual Assault Survivors},
  author = {Matthews, Kathleen C.},
  year = {2013},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {73},
  number = {9-B(E)},
  abstract = {Sexual assault is associated with a host of negative psychosocial outcomes. However, not all survivors of sexual assault experience these negative outcomes. Research is needed to more clearly identify both risk and protective factors for women's recovery after sexual assault. Social reactions to assault disclosure, attributions of responsibility for the sexual assault, and feelings of shame related to sexual assault are associated with sexual assault survivor outcomes. To date, no research has examined these factors within one study. In the present study, results of linear regression analyses supported the hypotheses that negative social reactions to disclosure, and, in particular, social reactions to disclosure that involve victim-blaming, would impact survivor attributions of responsibility, with greater negative reactions predicting more internal attributions of responsibility for the sexual assault. Furthermore, utilizing structural equation modeling, it appears that the associations among social reactions to disclosure and attributions of responsibility and psychological distress are all mediated by assault-related shame feelings as hypothesized. These results suggest that shame is a critical construct to attend to in the development of psychological distress following a sexual assault. These results also underscore the importance of targeting social reactions to disclosure, attributions of responsibility, and shame in research, intervention, and preventative efforts with sexual assault survivors. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {Attribution,attributions of responsibility,Emotional Adjustment,psychological adjustment,Sexual Abuse,sexual assault survivors,shame,Shame,social reactions,Survivors}
}

@article{maxwellNaturePredictorsSexual2003,
  title = {The {{Nature}} and {{Predictors}} of {{Sexual Victimization}} and {{Offending Among Adolescents}}},
  author = {Maxwell, Christopher D. and Robinson, Amanda L. and Post, Lori A.},
  year = {2003},
  month = dec,
  journal = {Journal of Youth and Adolescence},
  volume = {32},
  number = {6},
  pages = {465--477},
  issn = {1573-6601},
  doi = {10.1023/A:1025942503285},
  urldate = {2021-08-04},
  abstract = {This study describes the risk factors associated with experiencing and committing sexual aggression among a sample of male and female adolescents. High school students completed a questionnaire containing a revised form of the Sexual Experiences Survey to assess sexual victimization and offending experiences. Ordinal regression equations were estimated separately for male and female students, regressing background characteristics, dating behaviors, and attitudinal scales on sexual victimization (for females) or offending (for males). Nearly half (48\%) of the females report experiencing sexual aggression, and one-third (34\%) of males admit committing this type of offending. Regression analyses show that the likelihood of reporting victimization/offending increases among females who report dating more frequently during the past month, among both males and females who report dating more different people during the past 6 months, and among older males. Females who report their religious affiliation as Protestants compared to those with no religious affiliation, and those planning to attend college are less likely to report victimization. Among the males, rejecting rape stereotypes and having more accurate legal knowledge regarding rape are related to reduced likelihoods of reporting sexual offending. Implications for improving sexual assault educational programs for adolescents are discussed.},
  langid = {english},
  file = {/Users/sbward/Zotero/storage/B9AS45DV/Maxwell et al. - 2003 - The Nature and Predictors of Sexual Victimization .pdf}
}

@phdthesis{mayhallChildSexualAbuse2014,
  title = {Child Sexual Abuse Disclosure to a Parent or Legal Guardian a Phenomenological Study with an Emphasis on Depth Psychology},
  author = {Mayhall, Kerry},
  year = {2014},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {74},
  number = {7-B(E)},
  abstract = {The goal of a child sex predator is to silence the victim. To insure their protection, sex offenders will say or do almost anything to stop a child from telling. In response, sex education provided at an early age in schools encourages child victims to tell an adult if they are being hurt. Adult victims of child sexual abuse are increasingly becoming more active in speaking out against their abuse, finding their voice, and fighting back. Social media sites, interne blogs, and other victim websites have allowed a forum for victims to speak out and tell about their abuse, in efforts to bring awareness to the silent epidemic of child sexual abuse. The faces of perpetrators are displayed on sex offender watch websites and the media focuses on the life of the offender in an effort to understand the reason 'why.' The above notwithstanding, children still evidence a reluctance to disclose abuse. This study provides an in-depth look at the experience of child sexual abuse disclosure to a parent or legal guardian. Four female victims of child sexual abuse were interviewed and the data analyzed. Common themes were highlighted related to barriers in disclosure, such as low self-esteem and low self-worth, parentification, lack of a 'good enough mother,' and attachment style. Areas for future research and ideas for intervention related to increasing child sexual abuse disclosure were identified. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {Child Abuse,child sexual abuse,depth psychology,Depth Psychology,Guardianship,legal guardian,phenomenological study,Phenomenology,self esteem,Self-Esteem,sex offenders,Sexual Abuse,Silence}
}

@article{maynardElectroconvulsiveSeizuresInfluence2018,
  title = {Electroconvulsive Seizures Influence Dendritic Spine Morphology and {{BDNF}} Expression in a Neuroendocrine Model of Depression},
  author = {Maynard, {\relax Kristen}. R. and Hobbs, John W. and Rajpurohit, Sumita K. and Martinowich, Keri},
  year = {2018},
  month = jul,
  journal = {Brain Stimulation},
  volume = {11},
  number = {4},
  pages = {856--859},
  publisher = {Elsevier Science},
  issn = {1935-861X},
  doi = {10.1016/j.brs.2018.04.003},
  abstract = {Background: Electroconvulsive therapy (ECT) is a rapid and effective treatment for major depressive disorder. Chronic stress-induced depression causes dendrite atrophy and deficiencies in brain-derived neurotrophic factor (BDNF), which are reversed by anti-depressant drugs. Electroconvulsive seizures (ECS), an animal model of ECT, robustly increase BDNF expression and stimulate dendritic outgrowth. Objective: The present study aims to understand cellular and molecular plasticity mechanisms contributing to the efficacy of ECS following chronic stress-induced depression. Methods: We quantify Bdnf transcript levels and dendritic spine density and morphology on cortical pyramidal neurons in mice exposed to vehicle or corticosterone and receiving either Sham or ECS treatment. Results: ECS rescues corticosterone-induced defects in spine morphology and elevates Bdnf exon 1 and exon 4-containing transcripts in cortex. Conclusions: Dendritic spine remodeling and induction of activity-induced BDNF in the cortex represent important cellular and molecular plasticity mechanisms underlying the efficacy of ECS for treatment of chronic stress-induced depression. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  keywords = {Brain Derived Neurotrophic Factor,Brain-derived neurotrophic factor (BDNF),Cingulate cortex,Dendrites,Dendritic spine,Dendritic Spines,ECT,Electroconvulsive seizures,Electroconvulsive Shock Therapy,Major Depression,Mice,Morphology,Seizures}
}

@misc{McCabeHipwellKeenan,
