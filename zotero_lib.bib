@article{andersonInitialTestTacticFirst2021,
  title = {An {{Initial Test}} of the {{Tactic-First}} and {{Item-Order Hypotheses}}: {{Accounting}} for {{Response Discrepancies}} in {{Sexual Victimization Questionnaires}}},
  shorttitle = {An {{Initial Test}} of the {{Tactic-First}} and {{Item-Order Hypotheses}}},
  author = {Anderson, RaeAnn E. and Goodman, Erica L. and Ciampaglia, Alyssa M.},
  year = {2021},
  month = feb,
  journal = {American Journal of Criminal Justice},
