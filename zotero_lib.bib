@article{jacques-tiuraVictimizationFoodAddiction2021,
  title = {Victimization and {{Food Addiction Symptoms}}: {{Direct}} and {{Indirect Effects Through Emotion Dysregulation}}, {{Impulsivity}}, and {{Loss-of-Control Eating}}},
