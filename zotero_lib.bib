@article{parkhillHowSexualAssault2009,
  title = {How Do Sexual Assault Characteristics Vary as a Function of Perpetrators' Level of Intoxication?},
  author = {Parkhill, Michele R. and Abbey, Antonia and {Jacques-Tiura}, Angela J.},
  year = {2009},
