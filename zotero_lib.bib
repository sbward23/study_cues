@article{liscoRoleHeavyEpisodic2012,
  title = {The {{Role}} of {{Heavy Episodic Drinking}} and {{Hostile Sexism}} in {{Men}}'s {{Sexual Aggression Toward Female Intimate Partners}}},
  author = {Lisco, Claire G. and Parrott, Dominic J. and Tharp, Andra Teten},
