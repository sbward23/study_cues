@phdthesis{larenasIntergenerationalDifferencesCourse2004,
  title = {Intergenerational Differences in the Course of Alcoholism in Women:  {{An}} Examination of Female Drinking Patterns and the Progression of Alcohol Dependence},
  author = {Larenas, Mary N.},
  year = {2004},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {65},
  number = {3-B},
  pages = {1532},
  abstract = {Studies of differences between men and women in the course of alcohol dependence have revealed disparities, showing that women often have a shorter course of alcohol dependence and experience consequences more rapidly, a phenomenon known as telescoping. However, intragroup differences among women have rarely been studied. This study examined 310 predominantly Caucasian, middle-class, adult women with past DSM-IV alcohol dependence, recruited from across the United States as part of a large family study on the genetics of alcoholism. The sample was stratified into quartiles for each of two variables, first based on age of onset of alcohol dependence and second based on calendar year (generation) of onset of alcohol dependence. It was hypothesized that the course of alcohol dependence would differ among women who developed alcohol dependence earlier in their lifetimes and in earlier generations when compared to women whose age of onset of alcohol dependence was later and whose calander year of onset of alcohol dependence was more recent. One-way ANOVA's revealed that women developing alcohol dependence earlier in life and women developing alcohol dependence in earlier generations had (1) shorter durations from onset of regular drinking to onset of alcohol dependence, and (2) longer durations of alcohol dependence and (3) longer intervals from onset of dependence to treatment attempts, then women developing alcohol dependence later in life, or in later generations. These results suggest that women developing alcohol dependence later in life, and women developing alcohol dependence in later generations, lost control less rapidly and remain out of control for a shorter period, attempting treatment and recovering more quickly, resulting in a shorter or more compressed (telescoped) course of alcohol dependence. The cross-sectional nature of this study prevents conclusions of a causative nature, however we hypothesized that differences observed among age of onset groups could be due to lower severity in later onset women, whereas differences observed in generation groups might reflect more prevention, greater treatment options, and less stigmatization and gender stereotyping of women alcoholics in recent generations. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {alcohol dependence,Alcohol Drinking Patterns,alcoholism,Alcoholism,female drinking patterns,intergenerational differences,Intergenerational Relations}
}

