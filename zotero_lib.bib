@article{dawsonChangesAlcoholConsumption2015,
  title = {Changes in Alcohol Consumption: {{United States}}, 2001--2002 to 2012--2013},
  shorttitle = {Changes in Alcohol Consumption},
  author = {Dawson, Deborah A. and Goldstein, Ris{\"e} B. and Saha, Tulshi D. and Grant, Bridget F.},
