@incollection{cunninghamShameGuiltPTSD2020,
  title = {Shame and Guilt in {{PTSD}}},
  booktitle = {Emotion in Posttraumatic Stress Disorder: {{Etiology}}, Assessment, Neurobiology, and Treatment.},
  author = {Cunningham, Katherine C.},
  editor = {Tull, Matthew T. and Kimbrel, Nathan A.},
  year = {2020},
  pages = {145--171},
  publisher = {Elsevier Academic Press},
  address = {San Diego, CA},
  doi = {10.1016/B978-0-12-816022-0.00006-5},
  abstract = {The associations of shame and guilt with posttraumatic stress disorder (PTSD) have been well established, particularly among samples exposed to military trauma, including combat, military sexual trauma, or moral injury, or interpersonal traumas. Research shows that guilt and shame play a role in PTSD severity and maintenance. Shame and guilt have been consistently associated with PTSD symptom severity, and emerging research suggests these emotions may play a causal role in the maintenance of PTSD. Within the context of PTSD, shame and guilt have also been associated with myriad additional deleterious outcomes, including suicidal ideation and behavior. This chapter explores the extant literature on guilt and shame in relationship to PTSD. It reviews the impact of empirically supported PTSD treatments on these emotions, and discusses complimentary treatment approaches and avenues for future research. Continuing research is needed to understand the functions of negative self-evaluative emotions in PTSD and to expand treatment approaches and improve effectiveness. Recognizing and addressing shame and guilt among trauma-exposed populations have the potential to greatly improve PTSD prevention and recovery. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  isbn = {978-0-12-816022-0},
  keywords = {Emotional Trauma,guilt,Guilt,interpersonal trauma,Military Personnel,military sexual trauma,military trauma,moral injury,Moral Injury,posttraumatic stress disorder,Posttraumatic Stress Disorder,Psychotherapy,shame,Shame,suicidal ideation,Suicidal Ideation,Trauma Treatment,treatment approaches}
}

