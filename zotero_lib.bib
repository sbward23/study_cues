@article{nayakLifetimeVictimizationYear2012,
  title = {Lifetime Victimization and Past Year Alcohol Use in a {{U}}.{{S}}. Population Sample of Men and Women Drinkers},
  author = {Nayak, Madhabika B. and Lown, E. Anne and Bond, Jason C. and Greenfield, Thomas K.},
  year = {2012},
