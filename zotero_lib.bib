}

@article{klapPrevalenceStrangerHarassment2019,
  title = {Prevalence of {{Stranger Harassment}} of {{Women Veterans}} at {{Veterans Affairs Medical Centers}} and {{Impacts}} on {{Delayed}} and {{Missed Care}}},
