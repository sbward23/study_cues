}

@article{oueisQualitativeAccountCoping2023,
  title = {A {{Qualitative Account}} of {{Coping Following Non-Consensual Sexual Experiences}} among {{Gay}}, {{Bisexual}}, and {{Other Men Who Have Sex}} with {{Men}}},
  author = {Oueis, Jeremy and McKie, Raymond M. and Reissing, Elke D.},
  year = {2023},
