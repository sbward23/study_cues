  author = {Dindia, Kathryn},
  editor = {Montgomery, Barbara M. and Baxter, Leslie A.},
  year = {1998},
  pages = {83--108},
  publisher = {Lawrence Erlbaum Associates Publishers},
  address = {Mahwah, NJ},
  abstract = {Stigmas include, but are not limited to, physical disability, membership in some stigmatized group, character defects that are manifested by some discrediting event in the person's past or present, and disease. The literature on stigma disclosure includes research on disclosure of homosexuality, HIV-positive status, AIDS, sexual abuse, drug addiction, alcoholism, mental illness, epilepsy, and other stigmatized conditions, as well as occupational stigmatization.  The purpose of this chapter is to examine stigma self-disclosure as a dialectical process. Fundamental dialectical assumptions are used to frame research findings in the literature on stigma disclosure and major theoretical and methodological implications are identified. The 4 shared assumptions of a dialectical perspective are discussed: contradiction, change, praxis, and totality. The author illustrates how disclosure of stigmatized identities is a dialectical phenomenon by invoking these assumptions. Finally, this dialectical view of stigma disclosure is generalized to all self-disclosure. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0-8058-2112-0},
  keywords = {dialectical approach to study of stigma disclosure,Dialectics,Self-Disclosure,Stigma}
}

@article{dinirahmayaniPredictionSexualViolence2024,
