@article{dekkersAdverseChildhoodExperiences2023,
  title = {Adverse {{Childhood Experiences}} and {{Treatability}} in {{Dutch Patients}} with {{Cluster B Personality Disorders Mandated}} to {{Forensic Clinical Care}}: {{A Comparison}} of {{Men Convicted}} for {{Sexual}} and {{Violent Offenses}}},
  shorttitle = {Adverse {{Childhood Experiences}} and {{Treatability}} in {{Dutch Patients}} with {{Cluster B Personality Disorders Mandated}} to {{Forensic Clinical Care}}},
  author = {Dekkers, Denise and {Keulen-de Vos}, Marije},
  year = {2023},
  journal = {International Journal of Forensic Mental Health},
  volume = {0},
  number = {0},
  pages = {1--12},
  publisher = {Routledge},
  issn = {1499-9013},
  doi = {10.1080/14999013.2023.2257641},
  urldate = {2023-09-29},
  abstract = {Adverse childhood experiences (ACEs) have been consistently linked to interpersonal violence and are highly prevalent in correctional populations, particularly in men who perpetrate (sexual) violent behavior. Although exposure to ACEs is common in men who perpetrate interpersonal violence, less is known about the differences between men who commit sexual versus non-sexual violence. This cross-sectional study reports on the differences in prevalence of adverse childhood experiences in Dutch men who committed sexual offenses (N = 29) versus those who committed (non-sexual) violent offenses (N = 29), all who have been mandated to forensic clinical care. Drawing on data from a RCT-study among male forensic psychiatric inpatients with cluster B personality disorders from seven forensic hospitals in the Netherlands, the study explored potential differences in ACEs, and its relationship to treatability in the first year of forensic treatment. No significant differences were found in prevalence of childhood abuse and neglect among men convicted for sexual offenses versus those convicted for violent offenses. In our study, ACEs were not predictive of risk relating to treatability at the start of forensic treatment. Our findings suggest that taking into account trauma histories is of equal importance for both offending groups. Implications regarding trauma-informed forensic care are discussed.},
  keywords = {Adverse childhood experiences (ACEs),mandated care,sexual violence,treatability,violence}
}

@article{demontignygauthierRiskTellingDyadic2019,
  title = {The Risk of {{Telling}}: {{A Dyadic Perspective}} on {{Romantic Partners}}' {{Responses}} to {{Child Sexual Abuse Disclosure}} and {{Their Associations}} with {{Sexual}} and {{Relationship}}  {{Satisfaction}}.},
  author = {{de Montigny Gauthier}, Laurence and {Vaillancourt-Morel}, Marie-Pier and Rellini, Alessandra and Godbout, Natacha and {Charbonneau-Lefebvre}, V{\'e}ronique and Desjardins, Fr{\'e}d{\'e}rique and Bergeron, Sophie},
  year = {2019},
  month = jul,
  journal = {Journal of marital and family therapy},
  volume = {45},
  number = {3},
  pages = {480--493},
  address = {United States},
  issn = {1752-0606 0194-472X},
  doi = {10.1111/jmft.12345},
  abstract = {Among 70 community couples who reported childhood sexual abuse (CSA) and disclosure to their partner, this study examined associations between survivors' perception of  partner responses to their disclosure, and both partners' sexual and relationship  satisfaction. Participants completed self-report questionnaires online. Results of  path analyses within an actor-partner interdependence model indicated that  survivors' perceived partner responses of emotional support to disclosure were  associated with their own and their partners' higher sexual satisfaction. Survivors'  perceived responses of being stigmatized/treated differently by the partner were  associated with their own and their partners' poorer relationship satisfaction.  Findings suggest that survivor-perceived partner responses to the disclosure of CSA  can have a positive and a negative impact on the sexual and relationship  satisfaction of both partners.},
  copyright = {{\copyright} 2018 American Association for Marriage and Family Therapy.},
  langid = {english},
  pmid = {30006975},
  keywords = {*Interpersonal Relations,*Self Disclosure,Adolescence,Adolescent,Adult,Adult Survivors of Child Abuse,Adult Survivors of Child Abuse/*psychology,Aged,Child Abuse,Child Abuse Sexual,Child Abuse Sexual -- Psychosocial Factors,Child Abuse Sexual/psychology,Child Abuse Survivors -- Psychosocial Factors,child sexual abuse,Couples,Dyadic Adjustment Scale,Female,Humans,Impact of Events Scale,Interpersonal Relations,Male,Middle Age,Middle Aged,Relationship Satisfaction,Romance,romantic partners,Scales,Self Disclosure,Sexual Abuse,sexual and relationship satisfaction,Sexual Behavior,Sexual Behavior/*psychology,Sexuality,Spouses,Spouses -- Psychosocial Factors,Spouses/*psychology,Young Adult}
