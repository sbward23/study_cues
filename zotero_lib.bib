  author = {Beauchaine, Theodore P. and Zisner, Aimee},
  year = {2017},
  month = sep,
  journal = {International Journal of Psychophysiology},
  series = {The {{Psychophysiology}} of {{Motivation}}: {{Body}} and {{Brain}} in {{Action}}},
  volume = {119},
