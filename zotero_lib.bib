  abstract = {Sexual desire is of importance to sexual health, functioning, and well-being. Although an increasing number of studies address disorders related to sexual functioning, there is still a limited understanding of the underlying individual factors affecting sexual desire. The aim of the current study was to investigate the effect of sexual shame, emotion regulation strategies, and gender on sexual desire. In order to investigate this, sexual desire, expressive suppression, cognitive reappraisal, and sexual shame was measured in 218 Norwegian participants using the Emotion Regulation Questionnaire-10, the Sexual Desire Inventory-2, and the Sexual Shame Index-Revised. A multiple regression analysis indicated that cognitive reappraisal predicted sexual desire, {$\beta$}\,=\,0.343, (218)\,=\,5.09, p\,{$<$}\,001, CI [0.407, 0.920], whereas sexual shame and expressive suppression were unrelated to sexual desire. Men scored significantly higher than women on expressive suppression, F(1, 216)\,=\,24.968, p\,{$<$}\,0.001; partial {$\eta$}2\,=\,0.104. The current study did not find any significant differences between women and men on cognitive reappraisal, sexual desire or sexual shame, all p\,{$>$}\,0.05. Results from the current study indicates that the inclination toward cognitive reappraisal as a preferred emotion regulation strategy may positively affect the strength of sexual desire.},
  langid = {english},
  pmcid = {PMC10006235},
  pmid = {36899212},
  keywords = {Emotional Regulation,Emotions,Female,Humans,Libido,Male,Sexual Behavior,Shame},
  file = {/Users/sbward/Zotero/storage/HG9TKGMA/Sævik and Konijnenberg - 2023 - The effects of sexual shame, emotion regulation an.pdf}
}

@article{sahooTelevisedViolenceSex2006,
