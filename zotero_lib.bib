  title = {Ecological Momentary Assessment in the Investigation of Craving and Substance Use in Daily Life: {{A}} Systematic Review},
  shorttitle = {Ecological Momentary Assessment in the Investigation of Craving and Substance Use in Daily Life},
  author = {Serre, Fuschia and Fatseas, Melina and Swendsen, Joel and Auriacombe, Marc},
