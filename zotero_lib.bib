}

@article{littletonUnacknowledgedRapeReVictimization2017,
  title = {Unacknowledged {{Rape}} and {{Re-Victimization Risk}}: {{Examination}} of {{Potential Mediators}}},
  shorttitle = {Unacknowledged {{Rape}} and {{Re-Victimization Risk}}},
  author = {Littleton, Heather and Grills, Amie and Layh, Marlee and Rudolph, Kelly},
