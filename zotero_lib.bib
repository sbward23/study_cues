@article{mcclaySocialSupportMediates2020,
  title = {Social {{Support Mediates}} the {{Association}} between {{Disclosure}} of {{Suicide Attempt}} and {{Depression}}, {{Perceived Burdensomeness}}, and {{Thwarted Belongingness}}.},
