}

@article{swartoutTrajectoriesMaleSexual2015,
  title = {Trajectories of Male Sexual Aggression from Adolescence through College: {{A}} Latent Class Growth Analysis},
  shorttitle = {Trajectories of Male Sexual Aggression from Adolescence through College},
  author = {Swartout, Kevin M. and Swartout, Ashlyn G. and Brennan, Carolyn L. and White, Jacquelyn W.},
  year = {2015},
  journal = {Aggressive Behavior},
  volume = {41},
