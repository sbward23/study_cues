  month = jul,
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {63},
  number = {1-B},
  pages = {547},
  abstract = {The association between women's histories of abuse (physical, emotional, and sexual) and their current depressive symptoms and lifetime history of depressive disorders was explored, as was the possible role of negative cognitions (dysfunctional attitudes, negative attributional style, hopelessness and negative self-worth) as mediators. Participants were 240 women who were part of a longitudinal study of children at risk for developing depression (Garber, 1996). One-hundred-eighty-five had a history of mood disorders, and 55 had no history of psychiatric disorder. Women ranged in age from 29-54 years old, with a mean age of 38.5. The sample was primarily lower middle to middle class. The psychiatric history of the women was measured using the Structured Clinical Interview for DSM-III-R (SCID). Information about the women's history of childhood abuse was obtained from the SCID, a Life Line account of the memorable events of their lives, and the Life Experiences Questionnaire, Revised (LEQ-R). Current depressive symptoms were assessed with the Beck Depression Inventory (BDI), the Brief Symptom Inventory (BSI), and the Positive and Negative Affectivity Scale (PANAS). Cognitions measured with the Dysfunctional Attitude Scale (DAS), the Attributional Styles Questionnaire (ASQ), the Adult Self-Perception Profile (ASPP), and the Hopelessness Scale (HS). Results of SAS procedure PROC CALIS (SAS, 1999) structural equation modeling indicated history of abuse was associated with depression in adulthood. Differences in the magnitude of the relation between depression and abuse did not differ significantly as a function of type of abuse. Effects of abuse on depression appeared to be mediated most by hopelessness, then by negative self-worth, and finally by dysfunctional attitudes. Overall, the results support the hypothesis that childhood abuse is related to adult depression, and negative cognitions partially mediate this relation. Different types of abuse appeared to be roughly equally mediated by cognitions. The mediating effect of cognitions differed as a function of type of cognition. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {adult depression,Child Abuse,childhood abuse,Cognitions,Human Females,Major Depression,negative cognitions,women}
}

@article{quiggSexualViolenceNightlife2020,
  title = {Sexual Violence and Nightlife: {{A}} Systematic Literature Review},
