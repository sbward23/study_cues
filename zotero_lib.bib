  doi = {10.1037/0022-3514.69.2.353},
  abstract = {We tested a model describing the characteristics of sexually aggressive men that may also be useful for understanding the causes of other antisocial acts against women. This model hypothesizes that sexual aggressors can be identified by two sets of characteristics, labeled hostile masculinity and impersonal sex. To test this model, we followed up a sample of men 10 years after first studying them when they were young adults. We sought to predict which men would be in distressed relationships with women, be aggressive sexually, be nonsexually aggressive, or some combination of these. These behaviors were measured not only by questioning the men themselves but also by questioning many of the men's female partners. Some couples' videotaped conversations were also analyzed. The data supported the ability of the model to predict behavior 10 years later. We also developed the model further and identified the common and unique characteristics contributing to sexual aggression as compared with the other conflictual behaviors studied. The data supported the usefulness of hierarchical modeling incorporating both general factors that contribute to various interpersonal conflicts as well as specific factors uniquely pertaining to dominance of women. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  keywords = {Aggressive Behavior,Followup Studies,Human Males,Male Female Relations,Models,Predictive Validity,Psychosexual Behavior,Statistical Validity},
  file = {/Users/sbward/Zotero/storage/IUEYYZ3S/Malamuth et al. - 1995 - Using the confluence model of sexual aggression to.pdf;/Users/sbward/Zotero/storage/C3ZB5WW6/1995-44088-001.html;/Users/sbward/Zotero/storage/IMXMXYKT/1995-44088-001.html}
}

@article{malatestaRecoveryAlcoholismManaging1992,
  title = {Recovery from {{Alcoholism}}: {{Managing Shame}} and {{Guilt}}},
  author = {Malatesta, Victor J.},
  year = {1992},
  month = oct,
  journal = {Contemporary Psychology},
  volume = {37},
  number = {10},
  pages = {1012--1012},
  publisher = {American Psychological Association},
  issn = {0010-7549},
  doi = {10.1037/031456},
  abstract = {Reviews the book, Shame, Guilt and Alcoholism: Treatment Issues in Clinical Practice by Ronald T. Potter-Efron (see record [rid]1990-98429-000[/rid]). The affect of shame is an understudied, yet recently overused, clinical phenomenon that may contribute to the conceptualization and treatment of a variety of clinical problems, ranging from addictions and paraphilias to social phobia and sexual traumas. Presenting his conceptual model, Potter-Efron develops the argument that alcoholism is closely linked with the emotions of shame and guilt and that their identification and management are central to effective treatment. Of the 15 remaining chapters of 16, 6 chapters are devoted to a developmental and treatment analysis of shame. He addresses the 'Shame/Pride Continuum' and focuses on 'shame-generating' messages and behaviors that operate in families of origin. Although potentially useful for addictions therapists who are unfamiliar with but need to interact with 12-step programs, this book takes only a small step toward fostering a greater alliance with more empirically oriented treatment approaches. As such, the strength of the book lies in its attempt to clarify clinically basic concepts in affect theory. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  keywords = {Alcohol Treatment,alcoholism,Alcoholism,effective treatment,guilt,Guilt,shame,Shame,Social Phobia,Treatment Effectiveness Evaluation}
}

