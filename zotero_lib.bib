}

@article{jozkowskiGenderDifferencesHeterosexual2014,
  title = {Gender {{Differences}} in {{Heterosexual College Students}}' {{Conceptualizations}} and {{Indicators}} of {{Sexual Consent}}: {{Implications}} for {{Contemporary Sexual Assault Prevention Education}}},
  shorttitle = {Gender {{Differences}} in {{Heterosexual College Students}}' {{Conceptualizations}} and {{Indicators}} of {{Sexual Consent}}},
  author = {Jozkowski, Kristen N. and Peterson, Zo{\"e} D. and Sanders, Stephanie A. and Dennis, Barbara and Reece, Michael},
