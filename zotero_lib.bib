@article{shinnTopiramateTreatmentSubstance2010,
  title = {Topiramate in the Treatment of Substance Related Disorders: A Critical Review of the Literature},
  shorttitle = {Topiramate in the Treatment of Substance Related Disorders},
  author = {Shinn, Ann K. and Greenfield, Shelly F.},
  year = {2010},
  month = may,
  journal = {The Journal of clinical psychiatry},
  volume = {71},
  number = {5},
  pages = {634--648},
  issn = {0160-6689},
  doi = {10.4088/JCP.08r04062gry},
  urldate = {2021-04-24},
  abstract = {Objective To critically review the literature on topiramate in the treatment of substance related disorders. Data Sources A PubMed search of human studies published in English through January 2009. Study Selection 26 articles were identified and reviewed; these studies examined topiramate in disorders related to alcohol, nicotine, cocaine, methamphetamine, opioids, ecstasy, and benzodiazepines. Data Extraction Study design, sample size, topiramate dose and duration, and study outcomes were reviewed. Data Synthesis There is compelling evidence for the efficacy of topiramate in the treatment of alcohol dependence. Two trials show trends for topiramate's superiority over oral naltrexone in alcohol dependence, while one trial suggests topiramate is inferior to disulfiram. Despite suggestive animal models, evidence for topiramate in treating alcohol withdrawal in humans is slim. Studies of topiramate in nicotine dependence show mixed results. Human laboratory studies that used acute topiramate dosing show that topiramate actually enhances the pleasurable effects of both nicotine and methamphetamine. Evidence for topiramate in the treatment of cocaine dependence is promising, but limited by small sample size. The data on opioids, benzodiazepines, and ecstasy are sparse. Conclusion Topiramate is efficacious for the treatment of alcohol dependence, but side effects may limit widespread use. While topiramate's unique pharmacodynamic profile offers a promising theoretical rationale for use across multiple substance related disorders, heterogeneity both across and within these disorders limits topiramate's broad applicability in treating substance related disorders. Recommendations for future research include exploration of genetic variants for more targeted pharmacotherapies.},
  pmcid = {PMC3736141},
  pmid = {20361908},
  keywords = {substance related disorders,substance use,topiramate},
  file = {/Users/sbward/Zotero/storage/9PTSZ6L5/Shinn and Greenfield - 2010 - Topiramate in the treatment of substance related d.pdf}
}

@phdthesis{shishidoPredictingAlcoholUse2016,
  title = {Predicting Alcohol Use and Problems from Developmental and Interpersonal Factors of Emotion Dysregulation: {{A}} Structural Equation Model},
  author = {Shishido, Hanako},
  year = {2016},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  volume = {76},
  number = {9-B(E)},
  abstract = {Childhood maltreatment is a strong risk factor for increased levels of alcohol use and alcohol-related problems. One possible explanation for the associations between childhood maltreatment and alcohol use and problems is the influence of attachment dysfunction and adulthood interpersonal problems. Childhood maltreatment is detrimental to the healthy development of self worth and the ability to trust others, frequently resulting in dysfunctional attachment patterns. Attachment styles developed in childhood continue to serve as a schema in adult interpersonal relationships. Thus, individuals with anxious attachment styles may continue to believe they are unworthy of acceptance from others. Hence, they are likely to be clingy and submissive (i.e., selfless) in significant relationships. On the other hand, individuals with avoidant attachment may continue to believe others are untrustworthy and are thus likely to be distant and careless of others (i.e., selfish). Either type of problematic interpersonal style driven by attachment insecurity may cause significant emotional distress. As a result, these individuals may be vulnerable to alcohol use and the development of alcohol-related problems. Despite the sound theoretical support, no study to date has examined this comprehensive mechanism. This study tested the associations between childhood maltreatment and alcohol use/problems via insecure attachment and interpersonal problems using Structural Equation Modeling. The aim of this study was to provide a comprehensive understanding of the childhood maltreatment-alcohol use/problems associations based on attachment and interpersonal theories. Further, this study aimed to provide understanding about the unique influences of specific types of childhood maltreatment on attachment dysfunction. Archival data from 559 undergraduate students was used to test the hypotheses. Childhood punishment significantly predicted alcohol-related problems via avoidant attachment and selfish interpersonal problems. Childhood sexual abuse directly predicted alcohol problems, indicating a presence of other mediating factors that were not tested in the current study. Childhood neglect did not predict alcohol problems. None of the childhood maltreatment subtypes predicted alcohol consumption. The current study demonstrated the role of childhood maltreatment in the development of distinct interpersonal schemas with subsequent influences on alcohol-related problems. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {0419-4217},
  school = {ProQuest Information \& Learning},
  keywords = {alcohol abuse,Alcohol Abuse,attachment behavior,Attachment Behavior,Child Abuse,child maltreatment,emotion dysregulation,emotional insecurity,Emotional Security}
}

