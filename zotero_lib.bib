  author = {Edison, Briana and Coulter, Robert W. S. and Miller, Elizabeth and Stokes, Lynissa R. and Hill, Ashley},
  year = {2022},
  month = feb,
  journal = {JOURNAL OF ADOLESCENT HEALTH},
  volume = {70},
