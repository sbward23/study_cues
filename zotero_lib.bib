  year = {2006},
  month = sep,
  journal = {Indian Journal of Community Psychology},
  volume = {2},
  number = {2},
  pages = {26--37},
  publisher = {Community Psychology Association of India},
  issn = {0974-2719},
  abstract = {The aim of the present study is to find out the impact of television programmes showing inordinate levels of sex and violence on the patterns of aggression among viewer boys and girls belonging to late childhood and preadolescence. The main independent variable was television programmes. Many television channels like Star TV, M TV, V TV, etc. telecast programmes containing scenes having a very high degree of explicit or implicit sex and violence. In comparison with them, Doordarshan telecasts simpler programmes, and is less inclined to show violence and sex. To highlight the impact such embarrassing programmes containing too much sex and violence have on aggression, regular viewers of such programmes on these channels were compared with regular viewers of Doordarshan. An equal number of boys and girls of the two viewer groups were identified from the two age brackets, i.e., 8 to 11 years (lower age group) and 12 to 15 years (higher age group). The scheme of research employed a 2 x 2 x 2 factorial design. In the sample, 40 subjects each were included in each cell of the design, and therefore, 320 units were taken as subjects. Aggression scale was administered on the subjects individually. The three-way ANOVA results reveal that in comparison to viewers exposed to lower levels of violence and sex, those viewers watching higher violence and sexual content displayed higher levels of aggression uniformly equally across both the age and the gender levels. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {aggression,Aggressive Behavior,human sex differences,Human Sex Differences,televised violence,Television,television programs,Violence}
}

@article{saladinPTSDSymptomSeverity2003,
  title = {{{PTSD}} Symptom Severity as a Predictor of Cue-Elicited Drug Craving in Victims of Violent Crime},
  author = {Saladin, Michael E and Drobes, David J and Coffey, Scott F and Dansky, Bonnie S and Brady, Kathleen T and Kilpatrick, Dean G},
  year = {2003},
