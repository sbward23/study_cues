@article{ayers-glasseyInvestigatingEmotionDysregulation2021,
  title = {Investigating Emotion Dysregulation and the Perseveration- and Flow-like Characteristics of {{ADHD}} Hyperfocus in {{Canadian}} Undergraduate Students},
  author = {{Ayers-Glassey}, Samantha and MacIntyre, Peter D.},
  year = {2021},
