@article{davisHealthImpactsViolence2020,
  title = {The Health Impacts of Violence Perpetrated by Police, Military and Other Public Security Forces on Gay, Bisexual and Other Men Who Have Sex with Men in {{El Salvador}}.},
  author = {Davis, Dirk A. and Morales, Giuliana J. and Ridgeway, Kathleen and Mendizabal, Modesto and Lanham, Michele and Dayton, Robyn and Cooke, Juana and Santi, Karin and Evens, Emily},
  year = {2020},
