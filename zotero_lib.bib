@article{garofaloDifficultiesEmotionRegulation2018,
  title = {Difficulties in Emotion Regulation and Psychopathic Traits in Violent Offenders},
  author = {Garofalo, Carlo and Neumann, Craig S. and Velotti, Patrizia},
  year = {2018},
  month = jul,
  journal = {Journal of Criminal Justice},
