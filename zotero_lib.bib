@article{aslundcShamingExperiencesAssociation2007,
  title = {Shaming Experiences and the Association between Adolescent Depression and Psychosocial Risk Factors.},
  author = {{Aslund C} and {Nilsson KW} and {Starrin B} and {Sj{\"o}berg RL}},
  year = {2007},
