@article{bresinAssociationsPsychopathicTraits2022,
  title = {Associations {{Between Psychopathic Traits}} and {{Laboratory-Based Aggression}}: {{Moderating Effects}} of {{Provocation}} and {{Distraction}}},
  shorttitle = {Associations {{Between Psychopathic Traits}} and {{Laboratory-Based Aggression}}},
  author = {Bresin, Konrad and Alexander, Caelan and S. Subramani, Olivia and J. Parrott, Dominic},
  year = {2022},
