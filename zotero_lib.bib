@article{celnikerCollegeStudentsPerceptions2022,
  title = {College {{Students}}' {{Perceptions}} of {{Ambiguous Hook-ups Involving Alcohol Intoxication}}},
  author = {Celniker, Jared B. and Rode, Jacob B. and Anderson, Katherine B. and Ma, Brianna and Ditto, Peter H.},
  year = {2022},
  month = oct,
  journal = {Sex Roles},
  volume = {87},
