@article{tracyMitigatingEffectsMaternal2018,
  title = {The Mitigating Effects of Maternal Social Support and Paternal Involvement on the Intergenerational Transmission of Violence},
  author = {Tracy, Melissa and Salo, Madeleine and Appleton, Allison A.},
