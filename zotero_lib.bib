}

@article{vriesPleaseTellMe2019,
  title = {``{{Please}} Tell Me What Happened'': {{A}} Descriptive Study on Prevalence, Disclosure and Characteristics of Victimization in People with a Psychotic Disorder},
  shorttitle = {``{{Please}} Tell Me What Happened''},
  author = {de Vries, Bertine and Pijnenborg, Gerdina H. M. and van der Stouwe, Elisabeth C. D. and Visser, Ellen and de Jong, Steven and therapy {and} outcome {survey (PHAMOUS)-investigators}, Pharmaco and {Bartels-Velthuis}, Agna A. and Bruggeman, Richard and Castelein, Stynke and J{\"o}rg, Frederike and Veling, Wim and Aleman, Andr{\'e} and van Busschbach, Jooske T.},
  year = {2019},
