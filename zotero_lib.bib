}

@article{mestonEffectsExpressiveWriting2013,
  title = {Effects of Expressive Writing on Sexual Dysfunction, Depression, and {{PTSD}} in Women with a History of Childhood Sexual Abuse: {{Results}} from a Randomized Clinical Trial},
