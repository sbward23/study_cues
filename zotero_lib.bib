@article{mulletEarlyTraumaLater2022,
  title = {Early {{Trauma}} and {{Later Sexual Victimization}} in {{College Women}}: {{A Multiple Mediation Examination}} of {{Alexithymia}}, {{Impulsivity}}, and {{Alcohol Use}}},
  shorttitle = {Early {{Trauma}} and {{Later Sexual Victimization}} in {{College Women}}},
  author = {Mullet, Natira and Hawkins, Lindsey G. and Tuliao, Antover P. and Snyder, Hailey and Holyoak, Derek and McGuire, Kimberly C. and Earl, Alicia K. and McChargue, Dennis},
  year = {2022},
  month = oct,
  journal = {Journal of Interpersonal Violence},
