  year = {2019},
  month = feb,
  journal = {Psychological trauma : theory, research, practice and policy},
  volume = {11},
  number = {2},
  pages = {207--215},
  address = {United States},
  issn = {1942-969X},
  doi = {10.1037/tra0000384},
  abstract = {OBJECTIVE: Although there are several evidence-based treatments that are effective in mitigating the symptoms of posttraumatic stress disorder (PTSD), rates of  nonresponse to treatment as well as treatment dropout remain high. In order to  determine which symptoms remain most problematic at the end of treatment for PTSD,  the present study examined residual symptoms in a treatment study that evaluated the  effects of cognitive processing therapy (CPT) and prolonged exposure (PE). METHOD:  The sample consisted of 108 female rape survivors who initially met criteria for  PTSD and who were then randomized into and completed either CPT or PE. The sample  was 71\% White and 25\% African American, with an average age of 32 years (SD = 9.98  years). RESULTS: Symptoms of PTSD, depression, guilt, and social adjustment were  evaluated in the sample of treatment completers, and findings suggest that overall  symptoms improved over the course of treatment. Results further suggested that  distress related to trauma reminders, detachment, and insomnia were the most common  residual PTSD symptoms at posttreatment, while self-blame, concerns about body  image, and fatigue were the most common residual symptoms of depression. The most  common residual symptom of guilt was lack of justification for actions that had to  be taken during the index event. CONCLUSIONS: This pattern of findings is in line  with past research suggesting that evidence-based practices are effective in the  treatment of PTSD, but underscores the need to continue to evaluate residual  symptoms to best assist clients in achieving full recovery. (PsycINFO Database  Record (c) 2019 APA, all rights reserved).},
  langid = {english},
  pmid = {29963892},
  keywords = {*Cognitive Behavioral Therapy,*Implosive Therapy,Adult,Black Persons -- Psychosocial Factors,Body Image,Body Image/psychology,Cognitive Processes,cognitive processing therapy,Cognitive Processing Therapy,Cognitive Therapy,Cognitive Therapy -- Methods,Depression -- Symptoms,Depression/etiology/therapy,Evidence-Based Practice,Exposure,Fatigue,Fatigue/etiology/therapy,Female,Follow-Up Studies,Guilt,Guilt -- Evaluation,Human,Humans,Insomnia,Posttraumatic Stress Disorder,prolonged exposure,Psychiatric Status Rating Scales,PTSD,Rape -- Psychosocial Factors,Rape/psychology/rehabilitation,refractory symptoms,residual symptoms,Schizophrenia,Social Adjustment,Social Adjustment -- Evaluation,Stimulus Duration,Stress Disorders Post-Traumatic -- Symptoms,Stress Disorders Post-Traumatic -- Therapy,Stress Disorders Post-Traumatic/etiology/*psychology/*therapy,Stress Psychological,Survivors -- Psychosocial Factors,Survivors/psychology,Symptoms,Treatment Outcome,Treatment Outcomes,White Persons -- Psychosocial Factors}
}

@article{laskyBingeDrinkingGreekLife2017,
