}

@article{stantonDistinctDimensionsDefining2018,
  title = {Distinct Dimensions Defining the {{Adult ADHD Self-Report Scale}}: {{Implications}} for Assessing Inattentive and Hyperactive/Impulsive Symptoms},
  shorttitle = {Distinct Dimensions Defining the {{Adult ADHD Self-Report Scale}}},
  author = {Stanton, Kasey and Forbes, Miriam K. and Zimmerman, Mark},
  year = {2018},
