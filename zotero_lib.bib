@incollection{diblasioRolePTSDUnderstanding2011,
  title = {The Role of {{PTSD}} in Understanding Child Allegations of Sexual Abuse},
  booktitle = {Post-Traumatic Syndromes in Childhood and Adolescence: {{A}} Handbook of Research and Practice.},
  author = {Di Blasio, Paola and Miragoli, Sarah and Procaccia, Rossella},
  editor = {Ardino, Vittoria},
  year = {2011},
  pages = {177--198},
  publisher = {Wiley Blackwell},
  abstract = {This empirical chapter presents a study aiming to investigate whether PTSD symptoms impact on the reliability of child abuse allegations via a specific structure of narratives that could mirror the presence of post-traumatic symptoms. The study was carried out on 80 allegations by sexually abused children (aged 4-17 yrs). Age influenced the complexity and content of children's accounts. The length of the account and its lexical and conceptual organization are affected by children's linguistic and cognitive development; findings mainly discriminate preschoolers (aged 4-6 yrs) from school-aged children (aged 7-10 and 11-17 yrs). Advanced communicative skills depend on the ability to understand intentions and beliefs and, in turn, to infer one's and others' mental states. For this reason, only school-age children are able to produce more sophisticated allegations referring to mental states. PTSD affects autobiographical narrative regarding shame because children at that point recall the involvement with the abuser. Aspects that are particularly difficult to recall come to the child's mind through sensorial details that intrude in the form of reexperiencing symptoms. The results of this research thus highlight the need to differentiate child sexual abuse experiences in terms of specific cognitive and emotional mechanisms involved with PTSD symptoms. Although all the depositions examined here were considered to be reliable during the criminal proceedings and all perpetrators were found guilty, children's accounts are fragmented and qualitatively different in PTSD cases. Therefore, ethical considerations are the issue if professionals consider the potential risk of revictimizing children who have not healed their traumatic experiences. It is the authors' opinion that children with current PTSD symptoms should be treated prior to testifying in court. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  isbn = {978-0-470-99769-7},
  keywords = {abuse allegations,Adjudication,age differences,Age Differences,autobiographical memory,Autobiographical Memory,Child Abuse,child sexual abuse,court testimony,Legal Testimony,memory,Memory,Narratives,Posttraumatic Stress Disorder,PTSD,reliability,Sexual Abuse}
}

