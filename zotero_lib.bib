  title = {Emergency {{Department Visits}} for {{Sexual Assault}} by {{Emerging Adults}}: {{Is Alcohol}} a {{Factor}}?},
  shorttitle = {Emergency {{Department Visits}} for {{Sexual Assault}} by {{Emerging Adults}}},
  author = {Tadros, Allison and Sharon, Melinda J. and Hoffman, Shelley M. and Davidov, Danielle M.},
