@article{jacques-tiuraDisclosureSexualAssault2010,
  title = {Disclosure of Sexual Assault: {{Characteristics}} and Implications for Posttraumatic Stress Symptoms among {{African American}} and {{Caucasian}} Survivors},
  author = {{Jacques-Tiura}, Angela J. and Tkatch, Rifky and Abbey, Antonia and Wegner, Rhiana},
