  keywords = {Adolescent,Alcohol,Alcohol Drinking,Decision Making,Experimental methods,Female,HIV Infections,Humans,Intention,Male,Meta-analysis,Risk-Taking,Sexual behavior,Sexual Behavior,Unprotected sex,Unsafe Sex,Young Adult}
}

